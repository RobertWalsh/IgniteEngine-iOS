{
  "view": {
    "attributes": {
      "_id": "menu",
      "status_bar_style": "dark",
      "bg.color": "#5a5a5a"
    },
    "actions": [
      {
        "_type": "Load",
        "on": "willFirstAppear",
        "enabled": true,
        "attributes": {
          "_target": "dataMenu"
        }
      }
    ],
    "controls": [
      {
        "_id": "layoutMenu",
        "_type": "Layout",
        "attributes": {
          "size.w": "100%",
          "size.h": "100%",
          "margin.left": 0,
          "layoutType": "float"
        },
        "controls": [
          {
            "_id": "layoutTable",
            "_type": "Layout",
            "attributes": {
              "layoutType": "float",
              "size.w": "100%",
              "size.h": "100%",
              "padding.top": 70,
              "bg.color": "#fafafa00"
            },
            "controls": [
              {
                "_id": "tableSettings",
                "_type": "TableView",
                "attributes": {
                  "sectionHeader.xpath": "type",
                  "sectionHeader.xpath.size.h": 20,
                  "sectionHeader.xpath.size.w": "100%",
                  "size.h": "100%",
                  "size.w": 275,
                  "separator.style": "none",
                  "bg.color": "#ff00ff00",
                  "datasource.id": "dataMenu",
                  "data.basepath": "entities",
                  "enabled": true,
                  "cell.size.h": 42,
                  "cell.size.w": 275,
                  "margin.top": 0,
                  "animate_reload": false
                },
                "section_header_controls": [
                  {
                    "_id": "layoutSectionHeader",
                    "_type": "Layout",
                    "actions": [
                      {
                        "_id": "textCategory",
                        "_type": "Text",
                        "actions": [],
                        "attributes": {
                          "alpha": 1,
                          "font": "HelveticaNeue-Light:16",
                          "text": "[[dataRow.type]]",
                          "align.v": "middle",
                          "layoutType": "absolute",
                          "color": "#8f8f8f",
                          "padding.left": 10,
                          "size.w": "100%"
                        }
                      }
                    ],
                    "attributes": {
                      "bg.color": "#ffffff50",
                      "size.w": 275,
                      "size.h": "100%"
                    }
                  }
                ],
                "controls": [
                  {
                    "_id": "buttonEmployeeList",
                    "_type": "Layout",
                    "actions": [
                      {
                        "_type": "Navigate",
                        "attributes": {
                          "to": "/examples/[[dataRow.name]].json"
                        },
                        "on": "touchUp"
                      },
                      {
                        "_type": "Event",
                        "attributes": {
                          "delay": 0.5,
                          "_target": "imageMenu",
                          "event_name": "close"
                        },
                        "on": "touchUp"
                      }
                    ],
                    "attributes": {
                      "alpha": 1,
                      "font": "[[app.font]]:20",
                      "padding.left": 0,
                      "size.w":275,
                      "size.h": 42
                    },
                    "controls": [
                      {
                        "_id": "textCategory",
                        "_type": "Text",
                        "actions": [],
                        "attributes": {
                          "alpha": 1,
                          "font": "FontAwesome:16",
                          "text": "[[dataRow.icon]]",
                          "align.v": "middle",
                          "layoutType": "absolute",
                          "color": "#8f8f8f",
                          "padding.left": 20,
                          "touch.color": "[[app.color.touch]]",
                          "size.w": 60
                        }
                      },
                      {
                        "_id": "textCategory",
                        "_type": "Text",
                        "actions": [],
                        "attributes": {
                          "alpha": 1,
                          "font": "HelveticaNeue-Light:18",
                          "text": "[[dataRow.name]]",
                          "align.v": "middle",
                          "layoutType": "absolute",
                          "padding.left": 60,
                          "color": "#8f8f8f",
                          "touch.color": "[[app.color.touch]]",
                          "size.w": "100%"
                        }
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "_id": "layoutHeader",
        "_type": "Layout",
        "attributes": {
          "visible": true,
          "size.w": "100%",
          "size.h": 70,
          "margin.top": 0,
          "layoutType": "float",
          "bg.color": "#666666"
        },
        "controls": [
          {
            "_id": "layoutButtonHome",
            "_type": "Layout",
            "controls": [
              {
                "_id": "textSignedInAs",
                "_type": "Text",
                "actions": [],
                "attributes": {
                  "font": "HelveticaNeue:14",
                  "margin.left": 20,
                  "text": "Welcome to ",
                  "color": "#a7a7a7"
                }
              },
              {
                "_id": "textSignedInAs",
                "_type": "Text",
                "actions": [],
                "attributes": {
                  "font": "HelveticaNeue:14",
                  "margin.left": 0,
                  "text": "IX",
                  "color": "#ffffff"
                }
              }
            ],
            "attributes": {
              "layoutFlow": "horizontal",
              "margin.bottom": 8,
              "layoutType": "absolute",
              "align.v": "bottom"
            }
          }
        ]
      }
    ],
    "datasources": [
      
    ]
  }
}