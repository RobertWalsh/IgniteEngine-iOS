{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "#ffffff",
            "padding.top": 90,
            "statusBar.style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control_location": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXTextInput"
                }
            },
            {
                "_id": "textInput",
                "_type": "TextInput",
                "attributes": {
                    "align.h": "center",
                    "align.v": "middle",
                    "keyboard.returnKey": "go",
                    "keyboardAdjustsScreen.enabled": true,
                    "layoutType": "absolute",
                    "multiline.enabled": true,
                    "placeholder.text": "Type something...",
                    "size.h": 400,
                    "size.w": 200,
                    "text.align": "left"
                }
            }
        ],
        "datasources": []
    }
}