{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "#f8f8f8",
            "padding.top": 90,
            "statusBar.style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control.url": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXFunction"
                }
            },
            {
                "_id": "layoutButtons",
                "_type": "Layout",
                "attributes": {
                    "align.h": "center",
                    "align.v": "middle",
                    "layoutType": "absolute"
                },
                "controls": [
                    {
                        "_id": "button0",
                        "_type": "Button",
                        "actions": [
                            {
                                "_type": "Function",
                                "attributes": {
                                    "_target": "actionSheetTest",
                                    "functionName": "show"
                                },
                                "on": "touchUp"
                            }
                        ],
                        "attributes": {
                            "bg.color": "cdcdcd",
                            "color": "6c6c6c",
                            "margin.top": 15,
                            "size.h": 50,
                            "size.w": 280,
                            "text": "function: show_sheet",
                            "touch.bg.color": "cdcdcd",
                            "touch.color": "6c6c6c50"
                        }
                    }
                ]
            },
            {
                "_id": "actionSheetTest",
                "_type": "ActionSheet",
                "actions": [
                    {
                        "_type": "Alert",
                        "attributes": {
                            "title": "Cancel Pressed"
                        },
                        "on": "cancelled"
                    },
                    {
                        "_type": "Alert",
                        "attributes": {
                            "title": "other pressed [[app.bundle.version]]"
                        },
                        "on": "other"
                    },
                    {
                        "_type": "Alert",
                        "attributes": {
                            "title": "someOther2 pressed"
                        },
                        "on": "someOther2"
                    },
                    {
                        "_type": "Alert",
                        "attributes": {
                            "title": "[destructiveButtonTitle](#//api/name/destructiveButtonTitle) pressed"
                        },
                        "on": "[destructiveButtonTitle](#//api/name/destructiveButtonTitle)_pressed"
                    }
                ],
                "attributes": {
                    "buttons.cancel": "cancelButtonTitle",
                    "buttons.destructive": "destructiveButtonTitle",
                    "buttons.others": "other,someOther2",
                    "style": "black.opaque",
                    "title": "sheetTitle"
                }
            }
        ],
        "datasources": []
    }
}