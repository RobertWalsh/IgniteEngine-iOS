{
  "view": {
    "attributes": {
      "_id": "IXCamera",
      "status_bar_style": "light"
    },
    "actions": [
      {
        "_type": "Function",
        "on": "didAppear",
        "attributes": {
          "_target": "cameraTest",
          "functionName": "start"
        }
      }
    ],
    "controls": [
      {
  "_id": "cameraTest",
  "_type": "Camera",
  "actions": [
    {
      "on": "did_capture_image",
      "_type": "Alert",
      "attributes": {
        "title": "did_capture_image"
      }
    },
    {
      "on": "did_finish_saving_capture",
      "_type": "Alert",
      "attributes": {
        "title": "did_finish_saving_capture"
      }
    }
  ],
  "attributes": {
    "camera": "rear",
    "size.h": "100%",
    "size.w": "100%"
  }
},
      {
        "_id": "button",
        "_type": "Button",
        "actions": [
          {
            "_type": "Function",
            "on": "touchUp",
            "attributes": {
              "_target": "cameraTest",
              "functionName": "capture_image"
            }
          }
        ],
        "attributes": {
          "size.w": 150,
          "size.h": 50,
          "color": "6c6c6c",
          "layoutType": "absolute",
          "bg.color": "cdcdcd",
          "touch.color": "6c6c6c50",
          "align.h": "center",
          "align.v": "middle",
          "touch.bg.color": "cdcdcd",
          "border.radius": 0,
          "text": "Capture image."
        }
      }
    ]
  }
}