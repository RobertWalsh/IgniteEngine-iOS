{
	"attributes": {
		"height": "100%",
		"layout_type": "float",
		"width": "100%"
	},
	"controls": [
		{
			"attributes": {
				"height": 369,
				"images.default": "/images/iphone_background_tv_208x369.png",
				"layout_type": "float",
				"margin.top": 59,
				"visible": true,
				"width": 208
			},
			"controls": [],
			"type": "Image"
		},
		{
			"actions": [
				{
					"attributes": {
						"function_name": [
							{
								"if": "[[mediaplayer_tv.isOn]]==1",
								"value": "stop"
							}
						],
						"id": "mediaplayer_tv"
					},
					"on": "touch",
					"type": "Function"
				},
				{
					"attributes": {
						"id": "mediaplayer_tv"
					},
					"on": "touch",
					"parameters": {
						"video": [
							{
								"if": "[[mediaplayer_tv.isOn]]==1 && '[[mediaplayer_tv.video]]'=='/videos/tv_ad_video.mp4'",
								"value": "/videos/tv_news_video.mp4"
							},
							{
								"if": "[[mediaplayer_tv.isOn]]==1 && '[[mediaplayer_tv.video]]'=='/videos/tv_news_video.mp4'",
								"value": "/videos/tv_skating_video.mp4"
							},
							{
								"if": "[[mediaplayer_tv.isOn]]==1 && '[[mediaplayer_tv.video]]'=='/videos/tv_skating_video.mp4'",
								"value": "/videos/tv_ad_video.mp4"
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"delay" : 0.1,
						"function_name": [
							{
								"if": "[[layout_tv.visible]]==1",
								"value": "goto"
							}
						],
						"id": "mediaplayer_tv"
					},
					"on": "touch_up",
					"parameters": {
						"seconds": "[[app('random_number.40')]]"
					},
					"type": "Function"
				},
				{
					"delay" : 0.6,
					"attributes": {
						"function_name": [
							{
								"if": "[[layout_tv.visible]]==1",
								"value": "play"
							}
						],
						"id": "mediaplayer_tv"
					},
					"on": "touch_up",
					"type": "Function"
				}
			],
			"attributes": {
				"enabled": false,
				"height": 80,
				"layout_type": "float",
				"margin.left": 0,
				"margin.top": 214,
				"width": 208
			},
			"id": "button_tv_channelchanger",
			"type": "Button"
		},
		{
			"actions": [
				{
					"attributes": {
						"delay" : 0.1,
						"function_name": [
							{
								"if": "[[layout_tv.visible]]==1",
								"value": "goto"
							}
						],
						"id": "mediaplayer_tv"
					},
					"on": "touch_up",
					"parameters": {
						"seconds": "[[app('random_number.40')]]"
					},
					"type": "Function"
				},
				{
					"attributes": {
						"id": "layout_tv"
					},
					"on": "touch_up",
					"parameters": {
						"visible": [
							{
								"if": "[[layout_tv.visible]]==1",
								"value": false
							},
							{
								"if": "[[layout_tv.visible]]==0",
								"value": true
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "mediaplayer_tv"
					},
					"on": "touch",
					"parameters": {
						"isOn": [
							{
								"if": "[[mediaplayer_tv.isOn]]==1",
								"value": false
							},
							{
								"if": "[[mediaplayer_tv.isOn]]==0",
								"value": true
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "button_tv_onoff"
					},
					"on": "touch_up",
					"parameters": {
						"images.default": [
							{
								"if": "[[layout_tv.visible]]==1",
								"value": "/images/button_iphone_toggleon_52x47.png"
							},
							{
								"if": "[[layout_tv.visible]]==0",
								"value": "/images/button_iphone_toggleoff_52x47.png"
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "button_tv_channelchanger"
					},
					"on": "touch_up",
					"parameters": {
						"enabled": [
							{
								"if": "[[mediaplayer_tv.isOn]]==1",
								"value": true
							},
							{
								"if": "[[mediaplayer_tv.isOn]]==0",
								"value": false
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"function_name": [
							{
								"if": "[[layout_tv.visible]]==0",
								"value": "stop"
							},
							{
								"if": "[[layout_tv.visible]]==1",
								"value": "play"
							}
						],
						"id": "mediaplayer_tv"
					},
					"on": "touch_up",
					"type": "Function"
				}
			],
			"attributes": {
				"height": 47,
				"images.default": "/images/button_iphone_toggleoff_52x47.png",
				"layout_type": "float",
				"margin.left": 112,
				"margin.top": 171,
				"width": 52
			},
			"id": "button_tv_onoff",
			"type": "Button"
		}
	],
	"id": "layout_iphone_right_tv",
	"type": "Layout"
}