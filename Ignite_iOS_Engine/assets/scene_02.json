{
  "view": {
    "attributes": {
      "color.background": "#00000000",
      "margin": 0,
      "padding": 0,
      "status_bar_style": "hidden"
    },
    "actions": [
      {
        "attributes": {
          "nav_stack_type": "push",
          "to": "scene_03.json"
        },
        "on": "screen_pan_right",
        "type": "Navigate"
      },
      {
        "attributes": {
          "id": "animated_garage"
        },
        "on": "will_appear",
        "parameters": {
          "visible": false
        },
        "type": "Modify"
      },
      {
        "attributes": {
          "event_name": "touch_up",
          "id": "layout_inteceptor"
        },
        "on": "did_appear",
        "type": "Event"
      },
      {
        "attributes": {
          "id": "image_ext_00",
          "delay": 11,
          "duration": 0.5
        },
        "on": "did_appear",
        "parameters": {
          "alpha": 1
        },
        "type": "Modify"
      },
      {
        "attributes": {
          "id": "layout_next",
          "delay": 13,
          "duration": 0.5
        },
        "on": "did_appear",
        "parameters": {
          "alpha": 1
        },
        "type": "Modify"
      },
      {
        "attributes": {
          "nav_stack_type": "pop",
          "to": "scene_01.json"
        },
        "on": "screen_pan_left",
        "type": "Navigate"
      }
    ],
    "controls": [
    {
        "attributes": {
          "height": "100%",
          "images.default": "/images/ext_00_1024x768.jpg",
          "layout_type": "absolute",
          "alpha": 1,
          "width": "100%"
        },
        "controls": [],
        "id": "image_ext_00",
        "type": "Image"
      },
      {
        "attributes": {
          "auto_play": true,
          "controls": "none",
          "height": "100%",
          "isOn": false,
          "layout_type": "absolute",
          "video": "/videos/scene_02.mov",
          "width": "100%"
        },
        "id": "mediaplayer_scene02",
        "type": "MediaPlayer"
      },
      {
        "attributes": {
          "height": "100%",
          "images.default": "/images/ext_00_1024x768.jpg",
          "layout_type": "absolute",
          "alpha": 0,
          "width": "100%"
        },
        "controls": [],
        "id": "image_ext_00",
        "type": "Image"
      },
      {
        "actions": [],
        "attributes": {
          "auto_play": true,
          "isPlaying": false,
          "number_of_loops": 0,
          "volume": 0.75,
          "sound_location": "/audio/home_mixdown.aac"
        },
        "id": "sound_home_mixdown",
        "type": "Sound"
      },
      {
        "actions": [
          {
            "attributes": {
              "function_name": "play",
              "id": "sound_beep"
            },
            "on": "touch_up",
            "type": "Function"
          },
          {
            "on": "touch_up",
            "type": "Navigate",
            "attributes": {
              "to": "scene_03.json"
            }
          },
          {
            "attributes": {
              "id": "sound_home_mixdown",
              "duration": 1
            },
            "on": "touch_up",
            "parameters": {
              "volume": 0
            },
            "type": "Modify"
          }
        ],
        "attributes": {
          "height": "100%",
          "layout_type": "float",
          "alpha": 0,
          "width": "100%"
        },
        "controls": [
          {
            "attributes": {
              "height": "100%",
              "images.default": "/images/ext_01_1024x768.jpg",
              "layout_type": "absolute",
              "width": "100%"
            },
            "controls": [],
            "id": "image_ext_01",
            "type": "Image"
          },
          {
            "actions": [],
            "attributes": {
              "alpha": 0,
              "enabled": true,
              "height": 62,
              "images.default": "/images/step_next_418x62.png",
              "layout_type": "absolute",
              "width": 418,
              "vertical_alignment": "bottom",
              "horizontal_alignment": "center",
              "margin.bottom": 26
            },
            "controls": [],
            "id": "image_step_work_2",
            "type": "Image"
          }
        ],
        "id": "layout_next",
        "type": "Layout"
      },
      {
        "id": "timer_next",
        "type": "Timer",
        "actions": [
          {
            "attributes": {
              "id": "image_step_work_2",
              "duration": 0.5
            },
            "on": "timer_fired",
            "parameters": {
              "alpha": 0.1
            },
            "type": "Modify"
          },
          {
            "attributes": {
              "id": "image_step_work_2",
              "duration": 1,
              "delay": 0.25
            },
            "on": "timer_fired",
            "parameters": {
              "alpha": 1
            },
            "type": "Modify"
          }
        ],
        "attributes": {
          "enabled": true,
          "repeats": true,
          "time_interval": 1.5
        }
      },
      {
        "actions": [],
        "attributes": {
          "auto_play": false,
          "isPlaying": false,
          "number_of_loops": 0,
          "volume": 1,
          "sound_location": "/audio/beep.mp3"
        },
        "id": "sound_beep",
        "type": "Sound"
      }
    ]
  }
}