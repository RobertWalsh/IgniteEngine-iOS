{
  "view": {
    "attributes": {
      "color.background": "#000000",
      "margin": 0,
      "padding": 0,
      "status_bar_style": "hidden"
    },
    "actions": [
      {
        "attributes": {
          "nav_stack_type": "push",
          "to": "scene_03.json"
        },
        "on": "screen_pan_right",
        "type": "Navigate"
      },
      {
        "attributes": {
          "id": "animated_garage"
        },
        "on": "will_appear",
        "parameters": {
          "visible": false
        },
        "type": "Modify"
      },
      {
        "attributes": {
          "event_name": "touch_up",
          "id": "layout_inteceptor"
        },
        "on": "did_appear",
        "type": "Event"
      },
      {
        "attributes": {
          "nav_stack_type": "pop",
          "to": "scene_01.json"
        },
        "on": "screen_pan_left",
        "type": "Navigate"
      }
    ],
    "controls": [
      {
        "actions": [
          {
            "attributes": {
              "event_name": "show",
              "id": "iphone_right"
            },
            "on": "touch_up",
            "type": "Event"
          }
        ],
        "attributes": {
          "auto_play": true,
          "controls": "none",
          "height": "100%",
          "isOn": false,
          "layout_type": "absolute",
          "video": "/videos/scene2.mov",
          "width": "100%"
        },
        "id": "mediaplayer_tv",
        "type": "MediaPlayer"
      }
    ]
  }
}