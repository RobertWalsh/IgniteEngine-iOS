{
    "view": {
        "attributes": {
            "color.background": "#000000",
            "margin": 0,
            "padding": 0,
            "status_bar_style": "hidden"
        },
        "actions": [{
            "on": "screen_pan_right",
            "type": "Alert",
            "attributes": {
                "title": "Screen Pan Right"
            }
        },
        {
            "on": "screen_pan_left",
            "type": "Alert",
            "attributes": {
                "title": "Screen Pan Left"
            }
        }],
        "controls": [
            {
                "attributes": {
                    "color": {
                        "background": "#00FF00"
                    },
                    "enabled": true,
                    "height": "100%",
                    "horizontal_scroll_enabled": false,
                    "layout_type": "absolute",
                    "width": "100%"
                },
                "controls": [
                    {
                        "actions": [],
                        "attributes": {
                            "height": "100%",
                            "images.default": "/images/car_0_1024x768.jpg",
                            "layout_type": "absolute",
                            "visible": true,
                            "width": "100%"
                        },
                        "controls": [],
                        "id": "image_ext_0_1024x768",
                        "type": "Image"
                    },
                    {
                        "actions": [],
                        "attributes": {
                            "height": "100%",
                            "images.default": "/images/ext_1_1024x768.jpg",
                            "layout_type": "absolute",
                            "alpha": 0,
                            "width": "100%"
                        },
                        "controls": [],
                        "id": "image_ext_1_1024x768",
                        "type": "Image"
                    },
                    {
                        "actions": [],
                        "attributes": {
                            "height": "100%",
                            "images.default": "/images/ext_2_1024x768.jpg",
                            "layout_type": "absolute",
                            "alpha": 0,
                            "width": "100%"
                        },
                        "controls": [],
                        "id": "image_ext_2_1024x768",
                        "type": "Image"
                    },
                    {
                        "actions": [],
                        "attributes": {
                            "height": "100%",
                            "images.default": "/images/ext_3_1024x768.jpg",
                            "layout_type": "absolute",
                            "alpha": 0,
                            "width": "100%"
                        },
                        "controls": [],
                        "id": "image_ext_3_1024x768",
                        "type": "Image"
                    }
                ],
                "id": "layout_main",
                "type": "Layout"
            },
            {
                "actions": [
                    {
                        "attributes": {
                            "nav_animation_type": "none",
                            "nav_stack_type": "push",
                            "to": "scene_01.json"
                        },
                        "on": "touch_up",
                        "type": "Navigate"
                    }
                ],
                "attributes": {
                    "alpha": 0.4,
                    "color.background": "#FF0000",
                    "text": "scene_01"
                },
                "type": "Text"
            },
            {
                "actions": [
                    {
                        "attributes": {
                            "nav_animation_type": "none",
                            "nav_stack_type": "push",
                            "to": "scene_02.json"
                        },
                        "on": "touch_up",
                        "type": "Navigate"
                    }
                ],
                "attributes": {
                    "alpha": 0.4,
                    "color.background": "#0000FF",
                    "text": "scene_02"
                },
                "type": "Text"
            },
            {
                "actions": [
                    {
                        "attributes": {
                            "nav_animation_type": "none",
                            "nav_stack_type": "push",
                            "to": "scene_03.json"
                        },
                        "on": "touch_up",
                        "type": "Navigate"
                    }
                ],
                "attributes": {
                    "alpha": 0.4,
                    "color.background": "#00FF00",
                    "text": "scene_03"
                },
                "type": "Text"
            },
            {
                "actions": [
                    {
                        "attributes": {
                            "id": "image_ext_1_1024x768",
                            "duration": 0.25
                        },
                        "on": "touch_up",
                        "parameters": {
                            "alpha": [
                                {
                                    "if": "[[image_ext_1_1024x768.alpha]]==1",
                                    "value": 0
                                },
                                {
                                    "if": "[[image_ext_1_1024x768.alpha]]==0",
                                    "value": 1
                                }
                            ]
                        },
                        "type": "Modify"
                    }
                ],
                "attributes": {
                    "alpha": 0.4,
                    "color.background": "#ffffff",
                    "text": "Exterior Lights"
                },
                "type": "Text"
            },
            {
                "actions": [
                    {
                        "attributes": {
                            "id": "image_ext_2_1024x768",
                            "duration": 0.25
                        },
                        "on": "touch_up",
                        "parameters": {
                            "alpha": [
                                {
                                    "if": "[[image_ext_2_1024x768.alpha]]==1",
                                    "value": 0
                                },
                                {
                                    "if": "[[image_ext_2_1024x768.alpha]]==0",
                                    "value": 1
                                }
                            ]
                        },
                        "type": "Modify"
                    }
                ],
                "attributes": {
                    "alpha": 0.4,
                    "color.background": "#ffffff",
                    "text": "Garage"
                },
                "type": "Text"
            },
            {
                "actions": [
                    {
                        "attributes": {
                            "id": "image_ext_3_1024x768",
                            "duration": 0.25
                        },
                        "on": "touch_up",
                        "parameters": {
                            "alpha": [
                                {
                                    "if": "[[image_ext_3_1024x768.alpha]]==1",
                                    "value": 0
                                },
                                {
                                    "if": "[[image_ext_3_1024x768.alpha]]==0",
                                    "value": 1
                                }
                            ]
                        },
                        "type": "Modify"
                    }
                ],
                "attributes": {
                    "alpha": 0.4,
                    "color.background": "#ffffff",
                    "text": "Interior Lights"
                },
                "type": "Text"
            }
        ]
    }
