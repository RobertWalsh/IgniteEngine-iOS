{
	"attributes": {
		"height": "100%",
		"layout_type": "float",
		"width": "100%"
	},
	"controls": [
		{
			"attributes": {
				"height": 370,
				"images.default": "/images/iphone_background_music_208x369.png",
				"layout_type": "float",
				"margin.top": 59,
				"visible": true,
				"width": 208
			},
			"controls": [],
			"type": "Image"
		},
		{
			"actions": [
				{
					"attributes": {
						"id": "button_music_onoff"
					},
					"on": "touch_up",
					"parameters": {
						"images.default": [
							{
								"if": "[[mediaplayer_music.isPlaying]]==0",
								"value": "/images/button_iphone_toggleon_52x47.png"
							},
							{
								"if": "[[mediaplayer_music.isPlaying]]==1",
								"value": "/images/button_iphone_toggleoff_52x47.png"
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "mediaplayer_music"
					},
					"on": "touch_up",
					"parameters": {
						"isPlaying": [
							{
								"if": "[[mediaplayer_music.isPlaying]]==0",
								"value": true
							},
							{
								"if": "[[mediaplayer_music.isPlaying]]==1",
								"value": false
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"delay": 0.8,
						"function_name": [
							{
								"if": "[[mediaplayer_music.isPlaying]]==1",
								"value": "play"
							},
							{
								"if": "[[mediaplayer_music.isPlaying]]==0",
								"value": "pause"
							}
						],
						"id": "mediaplayer_music"
					},
					"on": "touch_up",
					"type": "Function"
				},
				{
					"attributes": {
						"id": "animation_sonos"
					},
					"on": "touch_up",
					"parameters": {
						"visible": true
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "animation_sonos",
						"delay": 0.75

					},
					"on": "touch_up",
					"parameters": {
						"visible": false
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"function_name": "play",
						"id": "sound_beep"
					},
					"on": "touch_up",
					"type": "Function"
				}
			],
			"attributes": {
				"height": 47,
				"images.default": "/images/button_iphone_toggleoff_52x47.png",
				"layout_type": "float",
				"margin.left": 126,
				"margin.top": 172,
				"width": 52
			},
			"id": "button_music_onoff",
			"type": "Button"
		},
		{
			"actions": [
				{
					"attributes": {
						"id": "mediaplayer_music"
					},
					"on": "value_changed",
					"parameters": {
						"volume": "[[slider_music_volume.value]]"
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "animation_sonos"
					},
					"on": "touch",
					"parameters": {
						"animation_duration": 0.5
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"function_name": "start_animation",
						"id": "animation_sonos"
					},
					"on": "value_changed",
					"type": "Function"
				},
				{
					"attributes": {
						"id": "animation_sonos"
					},
					"on": "touch",
					"parameters": {
						"animation_repeat_count": -1
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "animation_sonos"
					},
					"on": "touch_up",
					"parameters": {
						"animation_repeat_count": 1
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"id": "animation_sonos"
					},
					"on": "touch_up",
					"parameters": {
						"animation_duration": 1.6
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"function_name": "play",
						"id": "sound_beep"
					},
					"on": "touch_up",
					"type": "Function"
				}
			],
			"attributes": {
				"height": 20,
				"images.maximum": "/images/slider_images.max_20x22@2x.png",
				"images.maximum.capInsets": "{0,0,0,12}",
				"images.minimum": "/images/slider_images.min_20x22@2x.png",
				"images.minimum.capInsets": "{0,12,0,0}",
				"images.thumb": "/images/slider_thumb_23x24@2x.png",
				"initial_value": 1,
				"layout_type": "float",
				"margin.left": 24,
				"margin.top": 244,
				"maximum_value": 1,
				"minimum_value": 0,
				"width": 160
			},
			"id": "slider_music_volume",
			"type": "Slider"
		}
	],
	"id": "layout_iphone_right_music",
	"type": "Layout"
}