{
	"attributes": {
		"height": "100%",
		"layout_type": "float",
		"width": "100%"
	},
	"controls": [
		{
			"attributes": {
				"height": 369,
				"images.default": "/images/iphone_background_music_208x369.png",
				"layout_type": "float",
				"margin.top": 59,
				"visible": true,
				"width": 208
			},
			"controls": [],
			"type": "Image"
		},
		{
			"actions": [
				{
					"attributes": {
						"id": "mediaplayer_music"
					},
					"on": "touch_up",
					"parameters": {
						"isPlaying": [
							{
								"if": "[[mediaplayer_music.isPlaying]]==0",
								"value": true
							},
							{
								"if": "[[mediaplayer_music.isPlaying]]==1",
								"value": false
							}
						]
					},
					"type": "Modify"
				},
				{
					"attributes": {
						"function_name": [
							{
								"if": "[[mediaplayer_music.isPlaying]]==1",
								"value": "play"
							},
							{
								"if": "[[mediaplayer_music.isPlaying]]==0",
								"value": "pause"
							}
						],
						"id": "mediaplayer_music"
					},
					"on": "touch_up",
					"type": "Function"
				}
			],
			"attributes": {
				"color.background": "#0000FF",
				"height": 40,
				"layout_type": "float",
				"margin.top": 109,
				"width": 40
			},
			"id": "button_music_onoff",
			"type": "Button"
		},
		{
			"actions": [
				{
					"attributes": {
						"id": "mediaplayer_music"
					},
					"on": "value_changed",
					"parameters": {
						"volume": "[[slider_music_volume.value]]"
					},
					"type": "Modify"
				}
			],
			"attributes": {
				"height": 20,
				"images.maximum": "/images/slider_images.max_20x22@2x.png",
				"images.maximum.capInsets": "{0,0,0,12}",
				"images.minimum": "/images/slider_images.min_20x22@2x.png",
				"images.minimum.capInsets": "{0,12,0,0}",
				"images.thumb": "/images/slider_thumb_23x24@2x.png",
				"initial_value": 1,
				"layout_type": "float",
				"margin.left": 24,
				"margin.top": 200,
				"maximum_value": 1,
				"minimum_value": 0,
				"width": 160
			},
			"id": "slider_music_volume",
			"type": "Slider"
		}
	],
	"id": "layout_iphone_right_music",
	"type": "Layout"
}