{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "layoutButtons", 
                "_type": "Layout", 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "layoutType": "absolute"
                }, 
                "controls": [
                    {
                        "_id": "button0", 
                        "_type": "Button", 
                        "actions": [
                            {
                                "_type": "Delete", 
                                "attributes": {
                                    "_target": "button1"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "cdcdcd", 
                            "color": "6c6c6c", 
                            "size.h": 50, 
                            "size.w": 280, 
                            "touch.background.color": "cdcdcd", 
                            "touch.text.color": "6c6c6c50"
                        }
                    }, 
                    {
                        "_id": "button1", 
                        "_type": "Button", 
                        "actions": [
                            {
                                "_type": "Alert", 
                                "attributes": {
                                    "button.titles": "button0,button1", 
                                    "message": "You touched the button!"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "cdcdcd", 
                            "color": "6c6c6c", 
                            "size.h": 50, 
                            "size.w": 280, 
                            "touch.background.color": "cdcdcd", 
                            "touch.text.color": "6c6c6c50"
                        }
                    }
                ]
            }
        ], 
        "data_providers": []
    }
}