{
    "view": {
        "attributes": {
            "_id": "test", 
            "bg.color": "#f8f8f8", 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "layoutMain", 
                "_type": "Layout", 
                "attributes": {
                    "size.h": "100%", 
                    "size.w": "100%"
                }, 
                "controls": [
                    {
                        "_id": "layoutUrlEncode", 
                        "_type": "Layout", 
                        "attributes": {
                            "align.h": "center"
                        }, 
                        "controls": [
                            {
                                "_id": "textToUrlEncode", 
                                "_type": "Text", 
                                "attributes": {
                                    "color": "#000000", 
                                    "size.w": "100%", 
                                    "string": "This is a string.", 
                                    "text.align": "center"
                                }
                            }, 
                            {
                                "_id": "button", 
                                "_type": "Button", 
                                "actions": [
                                    {
                                        "_type": "Modify", 
                                        "attributes": {
                                            "_target": "textToUrlEncode"
                                        }, 
                                        "on": "touch_up", 
                                        "set": {}
                                    }
                                ], 
                                "attributes": {
                                    "align.h": "center", 
                                    "bg.color": "cdcdcd", 
                                    "color": "6c6c6c", 
                                    "size.h": 50, 
                                    "size.w": 200, 
                                    "touch.background.color": "cdcdcd", 
                                    "touch.text.color": "6c6c6c50"
                                }
                            }, 
                            {
                                "_id": "button", 
                                "_type": "Button", 
                                "actions": [
                                    {
                                        "_type": "Modify", 
                                        "attributes": {
                                            "_target": "textToUrlEncode"
                                        }, 
                                        "on": "touch_up", 
                                        "set": {}
                                    }
                                ], 
                                "attributes": {
                                    "align.h": "center", 
                                    "bg.color": "cdcdcd", 
                                    "color": "6c6c6c", 
                                    "size.h": 50, 
                                    "size.w": 200, 
                                    "touch.background.color": "cdcdcd", 
                                    "touch.text.color": "6c6c6c50"
                                }
                            }
                        ]
                    }
                ]
            }
        ], 
        "data_providers": []
    }
}