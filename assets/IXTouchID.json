{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 90,
      "background": {
        "color": "#ffffff"
      },
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "custom/header.json",
          "layout_type": "float",
          "height": 50,
          "width": "100%",
          "margin.top": 20,
          "title": "IXTouchID"
        }
      },
      {
        "_type": "TouchID",
        "_id": "touchIDTest",
        "actions": [
          {
            "_type": "Alert",
            "on": "success",
            "attributes": {
              "title": "Authenticated Successfully!"
            }
          }
        ],
        "attributes": {
          "title": "Please authenticate."
        }
      },
      {
        "_id": "button",
        "_type": "Button",
        "actions": [
          {
            "_type": "Function",
            "on": "touch_up",
            "attributes": {
              "_target": "touchIDTest",
              "function_name": "authenticate"
            }
          }
        ],
        "attributes": {
          "width": 200,
          "height": 50,
          "text.color": "6c6c6c",
          "layout_type": "absolute",
          "background.color": "cdcdcd",
          "touch.text.color": "6c6c6c50",
          "horizontal_alignment": "center",
          "vertical_alignment": "middle",
          "touch.background.color": "cdcdcd",
          "border.radius": 0,
          "text": "Papers, please!"
        }
      }
    ],
    "data_providers": []
  }
}