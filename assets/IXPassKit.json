{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "passKitTest", 
                "_type": "PassKit", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "pass.creation.success"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "pass.creation.failed"
                    }
                ], 
                "attributes": {
                    "passUrl": "/data/boardingpass.pkpass"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "passKitTest", 
                            "function_name": "pass.controller.present"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "bg.color": "cdcdcd", 
                    "color": "6c6c6c", 
                    "layoutType": "absolute", 
                    "size.h": 50, 
                    "size.w": 200, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }
        ], 
        "data_providers": []
    }
}