{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "mediaSourceTest", 
                "_type": "MediaSource", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "did_load_media"
                    }
                ], 
                "attributes": {
                    "cameraControls.enabled": true, 
                    "cameraSource": "front", 
                    "source": "library"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "mediaSourceTest", 
                            "function_name": "present_picker"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "bg.color": "cdcdcd", 
                    "color": "6c6c6c", 
                    "layoutType": "absolute", 
                    "size.h": 50, 
                    "size.w": 200, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }, 
            {
                "_id": "title", 
                "_type": "Text", 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "bottom", 
                    "color": "6c6c6c", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": 200, 
                    "text.align": "center"
                }
            }
        ], 
        "data_providers": []
    }
}