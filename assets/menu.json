{
    "view": {
        "actions": [], 
        "attributes": {
            "_id": "menu", 
            "bg.color": "#5a5a5a", 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "layoutMenu", 
                "_type": "Layout", 
                "attributes": {
                    "layoutType": "float", 
                    "size.h": "100%", 
                    "size.w": "100%"
                }, 
                "controls": [
                    {
                        "_id": "layoutTable", 
                        "_type": "Layout", 
                        "attributes": {
                            "bg.color": "#fafafa00", 
                            "layoutType": "float", 
                            "size.h": "100%", 
                            "size.w": "100%"
                        }, 
                        "controls": [
                            {
                                "_id": "tableSettings", 
                                "_type": "TableView", 
                                "attributes": {
                                    "bg.color": "#ff00ff00", 
                                    "cell.size.h": 42, 
                                    "datasource.id": "dataMenu", 
                                    "reloadAnimation.enabled": false, 
                                    "sectionHeader.size.h": 20, 
                                    "sectionHeader.size.w": "100%", 
                                    "sectionHeader.xpath": "type", 
                                    "size.h": "100%", 
                                    "size.w": "100%"
                                }, 
                                "controls": [
                                    {
                                        "_id": "buttonEmployeeList", 
                                        "_type": "Layout", 
                                        "actions": [
                                            {
                                                "_type": "Navigate", 
                                                "attributes": {
                                                    "to": "[[dataRow.view]]"
                                                }, 
                                                "on": "touch_up"
                                            }, 
                                            {
                                                "_type": "Event", 
                                                "attributes": {
                                                    "_target": "imageMenu", 
                                                    "delay": 0.5, 
                                                    "event_name": "close"
                                                }, 
                                                "on": "touch_up"
                                            }
                                        ], 
                                        "attributes": {
                                            "size.h": 42
                                        }, 
                                        "controls": [
                                            {
                                                "_id": "textCategory", 
                                                "_type": "Text", 
                                                "actions": [], 
                                                "attributes": {
                                                    "align.v": "middle", 
                                                    "color": "#8f8f8f", 
                                                    "layoutType": "absolute", 
                                                    "size.w": 60, 
                                                    "touch.text.color": "[[app.color.touch]]"
                                                }
                                            }, 
                                            {
                                                "_id": "textCategory", 
                                                "_type": "Text", 
                                                "actions": [], 
                                                "attributes": {
                                                    "align.v": "middle", 
                                                    "color": "#8f8f8f", 
                                                    "layoutType": "absolute", 
                                                    "size.w": "100%", 
                                                    "touch.text.color": "[[app.color.touch]]"
                                                }
                                            }
                                        ]
                                    }
                                ], 
                                "sectionHeader.controls": [
                                    {
                                        "_id": "layoutSectionHeader", 
                                        "_type": "Layout", 
                                        "actions": [
                                            {
                                                "_id": "textCategory", 
                                                "_type": "Text", 
                                                "actions": [], 
                                                "attributes": {
                                                    "align.v": "middle", 
                                                    "color": "#8f8f8f", 
                                                    "layoutType": "absolute", 
                                                    "size.w": "100%"
                                                }
                                            }
                                        ], 
                                        "attributes": {
                                            "bg.color": "#ffffff50", 
                                            "size.h": "100%", 
                                            "size.w": "100%"
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }, 
            {
                "_id": "layoutHeader", 
                "_type": "Layout", 
                "attributes": {
                    "bg.color": "#666666", 
                    "layoutType": "float", 
                    "size.h": 70, 
                    "size.w": "100%"
                }, 
                "controls": [
                    {
                        "_id": "layoutButtonHome", 
                        "_type": "Layout", 
                        "attributes": {
                            "align.v": "bottom", 
                            "layoutFlow": "horizontal", 
                            "layoutType": "absolute"
                        }, 
                        "controls": [
                            {
                                "_id": "textSignedInAs", 
                                "_type": "Text", 
                                "actions": [], 
                                "attributes": {
                                    "color": "#a7a7a7"
                                }
                            }, 
                            {
                                "_id": "textSignedInAs", 
                                "_type": "Text", 
                                "actions": [], 
                                "attributes": {
                                    "color": "#ffffff"
                                }
                            }
                        ]
                    }
                ]
            }
        ], 
        "data_providers": [
            {
                "_id": "dataMenu", 
                "_type": "JSON", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {
                            "messade": "Category: [[session.category_id]]\n\nRow Count: [[$self.count]]"
                        }, 
                        "on": "success"
                    }
                ], 
                "attributes": {
                    "auto_load": true, 
                    "cache_id": "menu", 
                    "data.basepath": "entities", 
                    "data.baseurl": "/data", 
                    "data.path": "menu.json", 
                    "http_method": "GET", 
                    "sort.key": "type", 
                    "sort.order": "ascending"
                }, 
                "parameters": {}
            }
        ]
    }
}