{
  "view": {
    "attributes": {
      "_id": "menu",
      "status_bar_style": "dark",
      "background.color": "#5a5a5a"
    },
    "actions": [
    ],
    "controls": [
      {
        "_id": "layoutMenu",
        "_type": "Layout",
        "attributes": {
          "width": "100%",
          "height": "100%",
          "margin.left": 0,
          "layout_type": "float"
        },
        "controls": [
          {
            "_id": "layoutTable",
            "_type": "Layout",
            "attributes": {
              "layout_type": "float",
              "width": "100%",
              "height": "100%",
              "padding.top": 70,
              "background.color": "#fafafa00"
            },
            "controls": [
              {
                "_id": "tableSettings",
                "_type": "TableView",
                "attributes": {
                  "height": "100%",
                  "width": "100%",
                  "separator.style": "none",
                  "background.color": "#ff00ff00",
                  "dataprovider_id": "dataMenu",
                  "enabled": true,
                  "item_height": 42,
                  "margin.top": 5,
                  "animate_reload": false
                },
                "controls": [
                  {
                    "_id": "buttonEmployeeList",
                    "_type": "Layout",
                    "actions": [
                      {
                        "_type": "Navigate",
                        "attributes": {
                          "to": "[[dataRow.name]].json"
                        },
                        "on": "touch_up"
                      },
                      {
                        "_type": "Event",
                        "attributes": {
                          "delay": 0.5,
                          "_target": "imageMenu",
                          "event_name": "close"
                        },
                        "on": "touch_up"
                      }
                    ],
                    "attributes": {
                      "alpha": 1,
                      "font": "[[app.font]]:20",
                      "padding.left": 0,
                      "height": 42
                    },
                    "controls": [
                      {
                        "_id": "textCategory",
                        "_type": "Text",
                        "actions": [],
                        "attributes": {
                          "alpha": 1,
                          "font": "FontAwesome:16",
                          "text":"[[dataRow.icon]]",
                          "vertical_alignment": "middle",
                          "layout_type": "absolute",
                          "text.color": "#8f8f8f",
                          "padding.left":20,
                          "touch.text.color": "[[app.color.touch]]",
                          "width": 60
                        }
                      },
                      {
                        "_id": "textCategory",
                        "_type": "Text",
                        "actions": [],
                        "attributes": {
                          "alpha": 1,
                          "font": "HelveticaNeue-Light:18",
                          "text": "[[dataRow.name]]",
                          "vertical_alignment": "middle",
                          "layout_type": "absolute",
                          "padding.left": 60,
                          "text.color": "#8f8f8f",
                          "touch.text.color": "[[app.color.touch]]",
                          "width": "100%"
                        }
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "_id": "layoutHeader",
        "_type": "Layout",
        "attributes": {
          "visible": true,
          "width": "100%",
          "height": 70,
          "margin.top": 0,
          "layout_type": "float",
          "background.color": "#666666"
        },
        "controls": [
          {
            "_id": "layoutButtonHome",
            "_type": "Layout",
            "controls": [
              {
                "_id": "textSignedInAs",
                "_type": "Text",
                "actions": [],
                "attributes": {
                  "font": "HelveticaNeue:14",
                  "margin.left": 20,
                  "text": "Welcome to ",
                  "text.color": "#a7a7a7"
                }
              },
              {
                "_id": "textSignedInAs",
                "_type": "Text",
                "actions": [],
                "attributes": {
                  "font": "HelveticaNeue:14",
                  "margin.left": 0,
                  "text": "IX",
                  "text.color": "#ffffff"
                }
              }
            ],
            "attributes": {
              "layout_flow": "horizontal",
              "margin.bottom": 8,
              "layout_type": "absolute",
              "vertical_alignment": "bottom"
            }
          }
        ]
      }
    ],
    "data_providers": [
      {
        "_id": "dataMenu",
        "_type": "JSON",
        "actions": [
          {
            "_type": "Alert",
            "attributes": {
              "messade": "Category: [[session.category_id]]\n\nRow Count: [[$self.count]]",
              "title": "admin_orders.json success",
              "enabled": false
            },
            "on": "success"
          }
        ],
        "attributes": {
          "auto_load": true,
          "data.baseurl": "/data",
          "data.path": "menu.json",
          "sort.order":"ascending",
          "sort.key":"name",
          "datarow.basepath": "entities",
          "cache_id": "menu",
          "http_method": "GET"
        },
        "parameters": {}
      }
    ]
  }
}