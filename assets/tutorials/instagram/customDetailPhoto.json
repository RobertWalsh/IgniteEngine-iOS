{
    "view": {
        "attributes": {}, 
        "controls": [
            {
                "_id": "layoutPhotoDetail", 
                "_type": "Layout", 
                "actions": [
                    {
                        "_type": "Modify", 
                        "attributes": {
                            "_target": "photoDetailOverlay"
                        }, 
                        "on": "touch_up", 
                        "set": {}
                    }, 
                    {
                        "_type": "Modify", 
                        "attributes": {
                            "_target": "photoDetailOverlay"
                        }, 
                        "on": "touch_up", 
                        "set": {}
                    }
                ], 
                "attributes": {
                    "size.h": 308, 
                    "size.w": 308
                }, 
                "controls": [
                    {
                        "_id": "imageAlreadyLoaded", 
                        "_type": "Image", 
                        "actions": [], 
                        "attributes": {
                            "image": "[[session.photoThumbUrl]]", 
                            "layoutType": "absolute", 
                            "size.h": 308, 
                            "size.w": 308
                        }
                    }, 
                    {
                        "_id": "imageStandardResolution", 
                        "_type": "Image", 
                        "actions": [], 
                        "attributes": {
                            "image": "[[session.photoUrl]]", 
                            "layoutType": "absolute", 
                            "size.h": 308, 
                            "size.w": 308
                        }
                    }
                ]
            }, 
            {
                "_id": "textCaption", 
                "_type": "Text", 
                "attributes": {
                    "align.h": "center", 
                    "color": "#ffffff", 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "textPhotographer", 
                "_type": "Text", 
                "attributes": {
                    "align.h": "center", 
                    "color": "#ffffff", 
                    "size.w": "100%"
                }
            }
        ]
    }
}