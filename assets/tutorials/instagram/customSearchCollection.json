{
  "view": {
    "controls": [
      {
        "_id": "collectionPhotos",
        "_type": "Collection",
        "actions": [
          {
            "_type": "Function",
            "on": "started_scrolling",
            "enabled": true,
            "attributes": {
              "_target": "inputSearch",
              "function_name": "keyboard_hide"
            }
          }
        ],
        "attributes": {
          "height": "100%",
          "width": "100%",
          "separator.style": "none",
          "background.color": "#ff00ff00",
          "dataprovider_id": "dataInstagram",
          "enabled": true,
          "item_height": 154,
          "item_width": 154,
          "margin.top": 0,
          "animate_reload": false
        },
        "controls": [
          {
            "_id": "layoutButtons",
            "_type": "Layout",
            "actions": [
              {
                "_type": "Function",
                "on": "touch_up",
                "enabled": true,
                "attributes": {
                  "_target": "inputSearch",
                  "function_name": "keyboard_hide"
                }
              },
              {
                "_type": "Modify",
                "on": "touch_up",
                "attributes": {
                  "_target": "session"
                },
                "set": {
                  "photoIndex": "[[dataRow.index]]",
                  "photoUrl": "[[dataRow.images.standard_resolution.url]]",
                  "photoCaption": "[[dataRow.caption.text]]",
                  "photoThumbUrl": "[[dataRow.images.low_resolution.url]]",
                  "photoPhotographer": "[[dataRow.user.username]]"
                }
              },
              {
                "_type": "Navigate",
                "on": "touch_up",
                "attributes": {
                  "to": "viewInstagramPhoto.json",
                  "1nav_stack_type": "push",
                  "1nav_animation_type": "cross_dissolve",
                  "1nav_animation_duration": 0
                }
              }
            ],
            "attributes": {
              "margin.top": 4,
              "height": "100%",
              "width": "100%",
              "background.color": "00ff0000"
            },
            "controls": [
              {
                "_id": "imageTest",
                "_type": "Image",
                "attributes": {
                  "width": "100%",
                  "height": "100%",
                  "images.default": "[[dataRow.images.low_resolution.url]]",
                  "layout_type": "absolute",
                  "vertical_alignment": "middle",
                  "horizontal_alignment": "center"
                }
              }
            ]
          }
        ]
      }
    ]
  }
}