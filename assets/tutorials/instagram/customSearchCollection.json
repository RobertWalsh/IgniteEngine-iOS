{
    "view": {
        "controls": [
            {
                "_id": "collectionPhotos", 
                "_type": "Collection", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "inputSearch", 
                            "function_name": "keyboard_hide"
                        }, 
                        "on": "started_scrolling"
                    }
                ], 
                "attributes": {
                    "bg.color": "#ff00ff00", 
                    "cell.size.h": 154, 
                    "cell.size.w": 154, 
                    "datasource.id": "dataInstagram", 
                    "reloadAnimation.enabled": false, 
                    "size.h": "100%", 
                    "size.w": "100%"
                }, 
                "controls": [
                    {
                        "_id": "layoutButtons", 
                        "_type": "Layout", 
                        "actions": [
                            {
                                "_type": "Function", 
                                "attributes": {
                                    "_target": "inputSearch", 
                                    "function_name": "keyboard_hide"
                                }, 
                                "on": "touch_up"
                            }, 
                            {
                                "_type": "Modify", 
                                "attributes": {
                                    "_target": "session"
                                }, 
                                "on": "touch_up", 
                                "set": {
                                    "photoCaption": "[[dataRow.caption.text]]", 
                                    "photoIndex": "[[dataRow.index]]", 
                                    "photoPhotographer": "[[dataRow.user.username]]", 
                                    "photoThumbUrl": "[[dataRow.images.low_resolution.url]]", 
                                    "photoUrl": "[[dataRow.images.standard_resolution.url]]"
                                }
                            }, 
                            {
                                "_type": "Navigate", 
                                "attributes": {
                                    "1nav_animation_duration": 0, 
                                    "1nav_animation_type": "cross_dissolve", 
                                    "1nav_stack_type": "push", 
                                    "to": "viewInstagramPhoto.json"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "00ff0000", 
                            "size.h": "100%", 
                            "size.w": "100%"
                        }, 
                        "controls": [
                            {
                                "_id": "imageTest", 
                                "_type": "Image", 
                                "attributes": {
                                    "align.h": "center", 
                                    "align.v": "middle", 
                                    "image": "[[dataRow.images.low_resolution.url]]", 
                                    "layoutType": "absolute", 
                                    "size.h": "100%", 
                                    "size.w": "100%"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}