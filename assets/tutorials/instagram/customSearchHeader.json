{
    "view": {
        "controls": [
            {
                "_id": "layoutHeader", 
                "_type": "Layout", 
                "attributes": {
                    "layoutType": "absolute", 
                    "size.h": 50, 
                    "size.w": "100%"
                }, 
                "controls": [
                    {
                        "_id": "title", 
                        "_type": "Text", 
                        "attributes": {
                            "align.h": "center", 
                            "align.v": "bottom", 
                            "color": "ffffff", 
                            "layoutType": "float", 
                            "size.h": 50, 
                            "size.w": "100%", 
                            "text.align": "center"
                        }
                    }, 
                    {
                        "_id": "imageLeft", 
                        "_type": "Image", 
                        "actions": [
                            {
                                "_type": "Navigate", 
                                "attributes": {
                                    "nav_stack_type": "pop"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "align.h": "left", 
                            "align.v": "middle", 
                            "backround.color": "ff00ff", 
                            "image": "./images/btn_back_44x44.png", 
                            "layoutType": "float", 
                            "size.h": 44, 
                            "size.w": 44
                        }
                    }, 
                    {
                        "_id": "imageRight", 
                        "_type": "Image", 
                        "actions": [
                            {
                                "_type": "Function", 
                                "attributes": {
                                    "_target": "socialTwitter", 
                                    "function_name": "present_share_controller"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "align.h": "right", 
                            "align.v": "middle", 
                            "backround.color": "ff00ff", 
                            "image": "./images/btn_tweet_44x44.png", 
                            "layoutType": "float", 
                            "size.h": 44, 
                            "size.w": 44
                        }
                    }
                ]
            }, 
            {
                "_id": "socialTwitter", 
                "_type": "Social", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "share_done"
                    }
                ], 
                "attributes": {
                    "image": "[[session.photoUrl]]", 
                    "platform": "twitter", 
                    "text": "[[session.photoCaption]] by @[[session.photoPhotographer]] on @instagram"
                }
            }
        ]
    }
}