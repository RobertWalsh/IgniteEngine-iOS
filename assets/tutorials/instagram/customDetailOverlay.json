{
    "view": {
        "actions": [
            {
                "_type": "Modify", 
                "attributes": {
                    "_target": "$custom"
                }, 
                "on": "touch_up", 
                "set": {}
            }, 
            {
                "_type": "Modify", 
                "attributes": {
                    "_target": "photoDetailOverlay", 
                    "delay": 0.25
                }, 
                "on": "touch_up", 
                "set": {}
            }
        ], 
        "attributes": {
            "bg.color": "#000000", 
            "layoutType": "float", 
            "size.h": "100%", 
            "size.w": "100%"
        }, 
        "controls": [
            {
                "_id": "layoutPhotoDetail", 
                "_type": "Layout", 
                "actions": [], 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "layoutType": "absolute", 
                    "size.h": 308, 
                    "size.w": 308
                }, 
                "controls": [
                    {
                        "_id": "imageStandardResolution", 
                        "_type": "Image", 
                        "actions": [], 
                        "attributes": {
                            "image": "[[session.photoUrl]]", 
                            "size.h": 308, 
                            "size.w": 308
                        }
                    }
                ]
            }
        ]
    }
}