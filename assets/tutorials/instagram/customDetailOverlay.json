{
  "view": {
    "attributes": {
      "layoutType": "float",
      "size.w": "100%",
      "size.h": "100%",
      "bg.color": "#000000"
    },
    "actions": [
      {
        "_type": "Modify",
        "on": "touchUp",
        "attributes": {
          "_target": "$custom",
          "duration": 0.25
        },
        "set": {
          "alpha": 0
        }
      },
      {
        "_type": "Modify",
        "on": "touchUp",
        "attributes": {
          "_target": "photoDetailOverlay",
          "delay": 0.25
        },
        "set": {
          "visible": false
        }
      }
    ],
    "controls": [
      {
        "_id": "layoutPhotoDetail",
        "_type": "Layout",
        "actions": [],
        "attributes": {
          "size.w": 308,
          "size.h": 308,
          "layoutType": "absolute",
          "align.h": "center",
          "align.v": "middle"
        },
        "controls": [
          {
            "_id": "imageStandardResolution",
            "_type": "Image",
            "actions": [],
            "attributes": {
              "size.w": 308,
              "size.h": 308,
              "image": "[[session.photoUrl]]"
            }
          }
        ]
      }
    ]
  }
}