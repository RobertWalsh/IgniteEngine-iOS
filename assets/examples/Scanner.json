{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "scannerControl", 
                "_type": "Scan", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "scanned"
                    }
                ], 
                "attributes": {
                    "audioUrl": "sounds/Slow Rise.m4r", 
                    "autoPlay.enabled": false, 
                    "repeatCount": 0
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "scannerControl", 
                            "function_name": "present_reader"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "bg.color": "cdcdcd", 
                    "border.size": 2, 
                    "color": "6c6c6c", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "soundControl", 
                            "function_name": "pause"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "bg.color": "cdcdcd", 
                    "border.size": 2, 
                    "color": "6c6c6c", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "soundControl", 
                            "function_name": "stop"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "bg.color": "cdcdcd", 
                    "border.size": 2, 
                    "color": "6c6c6c", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }
        ], 
        "data_providers": []
    }
}