{
    "view": {
        "attributes": {
            "background": {}, 
            "size.h": "100%", 
            "size.w": "100%"
        }, 
        "controls": [
            {
                "_id": "customButton", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {
                            "subtitle": "Button Example"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "darkenOnTouch.enabled": false, 
                    "images": {
                        "default": "images/image2.png", 
                        "default.tintColor": "#02220026", 
                        "disabled": "images/image-touch2.png", 
                        "disabled.tintColor": "#02220026", 
                        "touch.tintColor": "#02220026"
                    }, 
                    "layoutType": "relative", 
                    "size.h": "200", 
                    "size.w": "300"
                }
            }
        ]
    }
}