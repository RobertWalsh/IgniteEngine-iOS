{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 90,
      "bg.color":"#ffffff",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXCalendar"
        }
      },
      {
        "_type": "Calendar",
        "_id": "calendarTest",
        "actions": [
          {
            "_type": "Alert",
            "attributes": {
              "title": "success"
            },
            "on": "success"
          },
          {
            "_type": "Alert",
            "attributes": {
              "title": "error"
            },
            "on": "error"
          }
        ],
        "attributes": {
          "title": "Event Title",
          "allDay.enabled": false,
          "location": "1234 Some Street, City, State Zip",
          "notes": "Event Notes",
          "url": "http://meetings-are-fun.com",
          "date.format": "yyyy-MM-dd HH:mm:ss",
          "date.start": "2015-12-31 23:00:00",
          "date.end": "2016-01-01 00:00:00",
          "alarm.offset": -1800,
          "repeatFrequency": "none"
        }
      },
      {
        "_id": "button",
        "_type": "Button",
        "actions": [
          {
            "_type": "Function",
            "on": "touchUp",
            "attributes": {
              "_target": "calendarTest",
              "functionName": "addEvent"
            }
          }
        ],
        "attributes": {
          "size.w": 280,
          "size.h": 80,
          "color": "6c6c6c",
          "layoutType": "absolute",
          "bg.color": "cdcdcd",
          "touch.color": "6c6c6c50",
          "align.h": "center",
          "align.v": "middle",
          "touch.bg.color": "cdcdcd",
          "border.radius": 0,
          "text": "Add to Calendar: 10/21/2015"
        }
      }
    ],
    "datasources": []
  }
}