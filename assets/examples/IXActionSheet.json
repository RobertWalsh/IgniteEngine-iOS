{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 90,
      "bg.color":"#f8f8f8",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXActionSheet"
        }
      },
      {
        "_type": "ActionSheet",
        "_id": "actionSheetTest",
        "attributes": {
          "style": "black.opaque",
          "title": "sheetTitle",
          "buttons.cancel": "cancelButtonTitle",
          "buttons.destructive": "destructiveButtonTitle",
          "buttons.others": "other,someOther2"
        },
        "actions": [
          {
            "on": "cancel",
            "_type": "Alert",
            "attributes": {
              "title": "Cancel Pressed"
            }
          },
          {
            "on": "other",
            "_type": "Alert",
            "attributes": {
              "title": "other pressed [[app.bundle.version]]"
            }
          },
          {
            "on": "someOther2",
            "_type": "Alert",
            "attributes": {
              "title": "someOther2 pressed"
            }
          },
          {
            "on": "destructive",
            "_type": "Alert",
            "attributes": {
              "title": "destructiveButtonTitle pressed"
            }
          }
        ]
      },
      {
        "_id": "button",
        "_type": "Button",
        "actions": [
          {
            "_type": "Alert",
            "on": "touchUp",
            "enabled": false,
            "attributes": {
              "title": "touchUp",
              "message": "You touched the button!"
            }
          },
          {
            "_type": "Function",
            "on": "touchUp",
            "attributes": {
              "_target": "actionSheetTest",
              "functionName": "present"
            }
          }
        ],
        "attributes": {
          "size.w": 150,
          "size.h": 50,
          "color": "6c6c6c",
          "layoutType": "absolute",
          "bg.color": "cdcdcd",
          "touch.color": "6c6c6c50",
          "align.h": "center",
          "align.v": "middle",
          "touch.bg.color": "cdcdcd",
          "border.radius": 0,
          "text": "sheet."
        }
      }
    ],
    "datasources": []
  }
}