{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 70,
      "bg.color":"#ffffff",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXSocial"
        }
      },
      {
        "_id": "socialTest",
        "_type": "Social",
        "actions": [
          {
            "on": "success",
            "_type": "Alert",
            "attributes": {
              "title": "success"
            }
          }
        ],
        "attributes": {
          "platform": "twitter",
          "text": "I can't wait for you all to see this pic!",
          "url": "http://duck.lips",
          "image": "http://images.sodahead.com/slideshows/000020095/1537637670_ducklips87y6-103619834647_xlarge.png"
        }
      },
      {
        "_id": "button",
        "_type": "Button",
        "actions": [
          {
            "_type": "Function",
            "on": "touchUp",
            "attributes": {
              "_target": "socialTest",
              "functionName": "present"
            }
          }
        ],
        "attributes": {
          "size.w": 200,
          "size.h": 50,
          "color": "6c6c6c",
          "layoutType": "absolute",
          "bg.color": "cdcdcd",
          "touch.color": "6c6c6c50",
          "align.h": "center",
          "align.v": "middle",
          "touch.bg.color": "cdcdcd",
          "border.radius": 0,
          "text": "Share on Twitter"
        }
      },
      {
        "_id": "title",
        "_type": "Text",
        "attributes": {
          "size.w": 200,
          "size.h": 50,
          "margin.bottom": 15,
          "color": "6c6c6c",
          "layoutType": "float",
          "text.align": "center",
          "align.h": "center",
          "align.v": "bottom",
          "font": "HelveticaNeue:15",
          "text": "IXSocial: Twitter Available? \n[[socialTest.isAllowed.twitter]]"
        }
      }
    ],
    "datasources": []
  }
}