{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 90,
      "bg.color":"#ffffff",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXImage"
        }
      },
      {
        "_id": "imageTest",
        "_type": "Image",
        "actions": [
          {
            "_type": "Alert",
            "on": "touchUp",
            "attributes": {
              "title": "touchUp",
              "message": "Sized to [[$self.actual.size.w]]pt x [[$self.actual.size.h]]pt."
            }
          }
        ],
        "attributes": {
          "max.w": "100%",
          "image": "/images/bgs/storage_wars.jpg",
          "color": "6c6c6c",
          "image.blur.radius": 20,
          "layoutType": "absolute",
          "align.v": "middle",
          "align.h": "center"
        }
      },
      {
        "_id": "title",
        "_type": "Text",
        "attributes": {
          "size.w": "100%",
          "text.align": "center",
          "size.h": 50,
          "margin.bottom": 15,
          "color": "6c6c6c",
          "layoutType": "float",
          "align.h": "center",
          "align.v": "bottom",
          "font": "HelveticaNeue:15",
          "text": "IXImage: blurred 20pt"
        }
      }
    ],
    "datasources": []
  }
}