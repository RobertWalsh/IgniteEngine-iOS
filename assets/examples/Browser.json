{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_type": "Layout", 
                "attributes": {
                    "bg.color": "#cdcdcd", 
                    "layoutType": "float", 
                    "size.h": "{{ [[device.screen.height]] - 70}}", 
                    "size.w": "[[device.screen.width]]"
                }, 
                "controls": [
                    {
                        "_id": "browserChart", 
                        "_type": "Browser", 
                        "attributes": {
                            "size.h": "{{ [[device.screen.height]] - 70}}", 
                            "size.w": "[[device.screen.width]]", 
                            "url": "http://apigee.com"
                        }
                    }
                ]
            }
        ], 
        "data_providers": []
    }
}