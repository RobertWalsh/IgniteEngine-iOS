{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 70,
      "bg.color":"#ffffff",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXMap"
        }
      },
      {
        "_id": "mapControl",
        "_type": "Map",
        "actions": [
          {
            "on": "touchUp",
            "_type": "Alert",
            "attributes": {
              "title": "You touched on [[$self.annotation.title]]."
            }
          }
        ],
        "attributes": {
          "size.w": "100%",
          "size.h": "100%",
          "zoomLevel": 12,
          "animatePinDrop.enabled":true,
          "userLocation.enabled": true,
          "pin": {
            "title": "Title",
            "subtitle": "Subtitle",
            "lat": "37.333916",
            "long": "-121.894076"
          }
        }
      }
    ],
    "datasources": []
  }
}