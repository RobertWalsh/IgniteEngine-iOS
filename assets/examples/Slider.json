{
    "view": {
        "actions": [
            {
                "_type": "Function", 
                "attributes": {
                    "_target": "TextInput", 
                    "function_name": "keyboard_show"
                }, 
                "on": "did_appear"
            }
        ], 
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "customSlider", 
                "_type": "Slider", 
                "actions": [
                    {
                        "_type": "Refresh", 
                        "attributes": {
                            "_target": "labelText"
                        }, 
                        "on": "value_changed"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "value.default": 50, 
                    "value.max": 100, 
                    "value.min": 0
                }
            }, 
            {
                "_id": "labelText", 
                "_type": "Text", 
                "attributes": {
                    "color": "#6c6c6c", 
                    "size.h": 30, 
                    "size.w": "100%", 
                    "text.align": "center"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "customSlider", 
                            "function_name": "update_slider_value"
                        }, 
                        "on": "touch_up", 
                        "set": {
                            "animated": true
                        }
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "bg.color": "cdcdcd", 
                    "border.size": 2, 
                    "color": "6c6c6c", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }
        ], 
        "data_providers": []
    }
}