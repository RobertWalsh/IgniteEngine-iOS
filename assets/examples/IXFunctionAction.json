{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 90,
      "bg.color": "#f8f8f8",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXFunctionAction"
        }
      },
      {
        "_id": "layoutButtons",
        "_type": "Layout",
        "attributes": {
          "layoutType": "absolute",
          "align.h": "center",
          "align.v": "middle"
        },
        "controls": [
          {
            "_id": "button0",
            "_type": "Button",
            "actions": [
              {
                "_type": "Function",
                "on": "touchUp",
                "attributes": {
                  "_target": "actionSheetTest",
                  "functionName": "show_sheet"
                }
              }
            ],
            "attributes": {
              "size.w": 280,
              "size.h": 50,
              "color": "6c6c6c",
              "bg.color": "cdcdcd",
              "touch.color": "6c6c6c50",
              "touch.bg.color": "cdcdcd",
              "margin.top": 15,
              "text": "function: show_sheet"
            }
          }
        ]
      },
      {
        "_type": "ActionSheet",
        "_id": "actionSheetTest",
        "attributes": {
          "sheet.style": "black.opaque",
          "sheet.title": "sheetTitle",
          "sheet.button.title.cancel": "cancelButtonTitle",
          "sheet.button.title.destructive": "destructiveButtonTitle",
          "sheet.button.title.others": "other,someOther2"
        },
        "actions": [
          {
            "on": "cancel_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "Cancel Pressed"
            }
          },
          {
            "on": "other_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "other pressed [[app.bundle.version]]"
            }
          },
          {
            "on": "someOther2_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "someOther2 pressed"
            }
          },
          {
            "on": "[destructiveButtonTitle](#//api/name/destructiveButtonTitle)_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "[destructiveButtonTitle](#//api/name/destructiveButtonTitle) pressed"
            }
          }
        ]
      }
    ],
    "datasources": []
  }
}