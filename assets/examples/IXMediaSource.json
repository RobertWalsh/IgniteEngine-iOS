{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 70,
      "bg.color":"#ffffff",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXMediaSource"
        }
      },
      {
        "_id": "mediaSourceTest",
        "_type": "MediaSource",
        "actions": [
          {
            "on": "did_load_media",
            "_type": "Alert",
            "attributes": {
              "title": "did_load_media: [[$self.selected_media]]"
            }
          }
        ],
        "attributes": {
          "source": "library",
          "camera": "front",
          "show_camera_controls": true
        }
      },
      {
        "_id": "button",
        "_type": "Button",
        "actions": [
          {
            "_type": "Function",
            "on": "touchUp",
            "attributes": {
              "_target": "mediaSourceTest",
              "functionName": "present_picker"
            }
          }
        ],
        "attributes": {
          "size.w": 200,
          "size.h": 50,
          "color": "6c6c6c",
          "layoutType": "absolute",
          "bg.color": "cdcdcd",
          "touch.color": "6c6c6c50",
          "align.h": "center",
          "align.v": "middle",
          "touch.bg.color": "cdcdcd",
          "border.radius": 0,
          "text": "Pick something."
        }
      },
      {
        "_id": "title",
        "_type": "Text",
        "attributes": {
          "size.w": 200,
          "size.h": 50,
          "margin.bottom": 15,
          "color": "6c6c6c",
          "layoutType": "float",
          "text.align": "center",
          "align.h": "center",
          "align.v": "bottom",
          "font": "HelveticaNeue:15",
          "text": "IXMediaSource: Library"
        }
      }
    ],
    "datasources": []
  }
}