{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "#ffffff",
            "padding.top": 70,
            "status_bar_style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control_location": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXSound"
                }
            },
            {
                "_id": "soundTest",
                "_type": "AudioPlayer",
                "actions": [
                    {
                        "_type": "Alert",
                        "attributes": {
                            "message": "Played sound: resources/sounds/powerup.caf",
                            "title": "Finished"
                        },
                        "on": "finished"
                    }
                ],
                "attributes": {
                    "audioUrl": "resources/sounds/powerup.caf",
                    "autoPlay.enabled": false,
                    "repeatCount": 0
                }
            },
            {
                "_id": "button",
                "_type": "Button",
                "actions": [
                    {
                        "_type": "Function",
                        "attributes": {
                            "_target": "soundTest",
                            "functionName": "play"
                        },
                        "on": "touchUp"
                    },
                    {
                        "_type": "Refresh",
                        "attributes": {
                            "_target": "title",
                            "delay": 0.25
                        },
                        "on": "touchUp"
                    }
                ],
                "attributes": {
                    "align.h": "center",
                    "align.v": "middle",
                    "bg.color": "cdcdcd",
                    "border.radius": 0,
                    "color": "6c6c6c",
                    "layoutType": "absolute",
                    "size.h": 50,
                    "size.w": 200,
                    "text": "Power up.",
                    "touch.bg.color": "cdcdcd",
                    "touch.color": "6c6c6c50"
                }
            },
            {
                "_id": "title",
                "_type": "Text",
                "attributes": {
                    "align.h": "center",
                    "align.v": "bottom",
                    "color": "6c6c6c",
                    "font": "HelveticaNeue:15",
                    "layoutType": "float",
                    "margin.bottom": 15,
                    "size.h": 50,
                    "size.w": 280,
                    "text": "IXSound: is_playing: [[soundTest.is_playing]]\nPlay /resources/sounds/powerup.caf",
                    "text.align": "center"
                }
            }
        ],
        "datasources": []
    }
}