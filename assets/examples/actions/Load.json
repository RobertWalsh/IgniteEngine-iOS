{
    "view": {
        "actions": [],
        "attributes": {
            "_id": "test",
            "bg.color": "#f8f8f8",
            "padding.top": 70,
            "statusBar.style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control.url": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXLoad"
                }
            },
            {
                "_id": "tableDemo",
                "_type": "TableView",
                "attributes": {
                    "reloadAnimation.enabled": false,
                    "bg.color": "#ff00ff00",
                    "cell.size.h": 42,
                    "data.basepath": "entities",
                    "datasource.id": "dataMenu",
                    "enabled": true,
                    "margin.top": 0,
                    "padding.bottom": 65,
                    "separator.style": "none",
                    "size.h": "100%",
                    "size.w": 320
                },
                "controls": [
                    {
                        "_id": "buttonEmployeeList",
                        "_type": "Layout",
                        "actions": [
                            {
                                "_type": "Navigate",
                                "attributes": {
                                    "animation.duration": 0,
                                    "animation.type": "crossDissolve",
                                    "to": "[[dataRow.name]].json"
                                },
                                "on": "touchUp"
                            },
                            {
                                "_type": "Event",
                                "attributes": {
                                    "_target": "imageMenu",
                                    "delay": 0.5,
                                    "eventName": "close"
                                },
                                "on": "touchUp"
                            }
                        ],
                        "attributes": {
                            "alpha": 1,
                            "font": "[[app.font]]:20",
                            "padding.left": 0,
                            "size.h": 42,
                            "size.w": 320
                        },
                        "controls": [
                            {
                                "_id": "textCategory",
                                "_type": "Text",
                                "actions": [],
                                "attributes": {
                                    "align.v": "middle",
                                    "alpha": 1,
                                    "color": "#8f8f8f",
                                    "font": "FontAwesome:16",
                                    "layoutType": "absolute",
                                    "padding.left": 20,
                                    "size.w": 60,
                                    "text": "[[dataRow.icon]]",
                                    "touch.color": "[[app.color.touch]]"
                                }
                            },
                            {
                                "_id": "textCategory",
                                "_type": "Text",
                                "actions": [],
                                "attributes": {
                                    "align.v": "middle",
                                    "alpha": 1,
                                    "color": "#8f8f8f",
                                    "font": "HelveticaNeue-Light:18",
                                    "layoutType": "absolute",
                                    "padding.left": 60,
                                    "size.w": "100%",
                                    "text": "[[dataRow.name]]",
                                    "touch.color": "[[app.color.touch]]"
                                }
                            }
                        ]
                    }
                ]
            },
            {
                "_id": "layoutButtons",
                "_type": "Layout",
                "attributes": {
                    "align.h": "center",
                    "align.v": "bottom",
                    "layoutType": "absolute",
                    "padding.bottom": 15
                },
                "controls": [
                    {
                        "_id": "button0",
                        "_type": "Button",
                        "actions": [
                            {
                                "_type": "Load",
                                "attributes": {
                                    "_target": "dataMenu"
                                },
                                "enabled": true,
                                "on": "touchUp"
                            }
                        ],
                        "attributes": {
                            "bg.color": "cdcdcd",
                            "color": "6c6c6c",
                            "margin.top": 15,
                            "size.h": 50,
                            "size.w": 280,
                            "text": "load data: dataMenu",
                            "touch.bg.color": "cdcdcd",
                            "touch.color": "6c6c6c50"
                        }
                    }
                ]
            }
        ],
        "datasources": [
            {
                "_id": "dataMenu",
                "_type": "JSON",
                "actions": [
                    {
                        "_type": "Alert",
                        "attributes": {
                            "enabled": true,
                            "message": "Category: [[session.category_id]]\n\nRow Count: [[$self.count]]",
                            "title": "admin_orders.json success"
                        },
                        "on": "success"
                    },
                    {
                        "_type": "Refresh",
                        "attributes": {
                            "_target": "tableDemo"
                        },
                        "on": "success"
                    }
                ],
                "attributes": {
                    "autoLoad.enabled": false,
                    "baseUrl": "../data",
                    "cache.id": "menu",
                    "data.basepath": "entities",
                    "http.method": "GET",
                    "pathSuffix": "menu.json",
                    "sort.key": "name",
                    "sort.order": "ascending"
                },
                "parameters": {}
            }
        ]
    }
}