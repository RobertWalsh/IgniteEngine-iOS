{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "#f8f8f8",
            "padding.top": 90,
            "statusBar.style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control.url": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXNavigate"
                }
            },
            {
                "_id": "textInputTest",
                "_type": "TextInput",
                "actions": [
                    {
                        "_type": "Modify",
                        "attributes": {
                            "_target": "session"
                        },
                        "on": "text_changed,got_focus",
                        "set": {
                            "text_to_speak": "[[$self.text]]"
                        }
                    }
                ],
                "attributes": {
                    "align.h": "center",
                    "border.color": "cdcdcd",
                    "border.size.w": 1,
                    "color": "#6c6c6c",
                    "font": "HelveticaNeue-Light:22",
                    "is_multiline": false,
                    "size.h": 50,
                    "size.w": 150,
                    "text.align": "center",
                    "text.default": 0.75,
                    "text.placeholder": "Duration"
                }
            },
            {
                "_id": "layoutButtonsLeft",
                "_type": "Layout",
                "attributes": {
                    "align.h": "left",
                    "align.v": "middle",
                    "layoutType": "absolute",
                    "size.w": "50%"
                },
                "controls": [
                    {
                        "_id": "button0",
                        "_type": "Button",
