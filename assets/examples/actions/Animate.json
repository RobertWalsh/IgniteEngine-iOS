{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "ffffff",
            "padding.top": 90,
            "statusBar.style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control.url": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXAnimation"
                }
            },
            {
                "_id": "layoutButtons",
                "_type": "Layout",
                "attributes": {
                    "align.h": "center",
                    "align.v": "middle",
                    "layoutType": "absolute"
                },
                "controls": [
                    {
                        "_id": "button0",
                        "_type": "Button",
                        "actions": [
                            {
                                "_type": "Animate",
                                "attributes": {
                                    "_target": "layoutSpinMe",
                                    "animation": "spin",
                                    "duration": 1,
                                    "repeatCount": 0
                                },
                                "on": "touchUp"
                            }
                        ],
                        "attributes": {
                            "bg.color": "cdcdcd",
                            "color": "6c6c6c",
                            "margin.top": 15,
                            "size.h": 50,
                            "size.w": 280,
                            "text": "animate",
                            "touch.bg.color": "cdcdcd",
                            "touch.color": "6c6c6c50"
                        }
                    },
                    {
                        "_id": "button0",
                        "_type": "Button",
                        "actions": [
                            {
                                "_type": "Modify",
                                "attributes": {
                                    "_target": "circleTest"
                                },
                                "enabled": true,
                                "on": "touchUp",
                                "set": {
                                    "color": "293aba"
                                }
                            }
                        ],
                        "attributes": {
                            "bg.color": "cdcdcd",
                            "color": "6c6c6c",
                            "margin.top": 15,
                            "size.h": 50,
                            "size.w": 280,
                            "text": "change dot color",
                            "touch.bg.color": "cdcdcd",
                            "touch.color": "6c6c6c50"
                        }
                    }
                ]
            },
            {
                "_id": "layoutSpinMe",
                "_type": "Layout",
                "attributes": {
                    "align.h": "center",
                    "align.v": "bottom",
                    "bg.color": "ff00ff00",
                    "layoutType": "absolute",
                    "margin.bottom": 40,
                    "size.h": 40,
                    "size.w": 40
                },
                "controls": [
                    {
                        "_id": "circleTest",
                        "_type": "Circle",
                        "attributes": {
                            "align.h": "left",
                            "align.v": "top",
                            "color": "#fa4c06",
                            "layoutType": "absolute",
                            "size": 8
                        }
                    }
                ]
            }
        ],
        "datasources": []
    }
}