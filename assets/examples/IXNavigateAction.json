    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXNavigateAction"
        }
      },
      {
        "_id": "textInputTest",
        "_type": "TextInput",
        "actions": [
          {
            "_type": "Modify",
            "on": "text_changed,got_focus",
            "attributes": {
              "_target": "session"
            },
            "set": {
              "text_to_speak": "[[$self.text]]"
            }
          }
        ],
        "attributes": {
          "font": "HelveticaNeue-Light:22",
          "is_multiline": false,
          "size.w": 150,
          "size.h": 50,
          "text.align": "center",
          "align.h": "center",
          "color": "#6c6c6c",
          "border.size.w": 1,
          "border.color": "cdcdcd",
          "text.default":0.75,
          "text.placeholder": "Duration"
        }
      },
      {
        "_id": "layoutButtonsLeft",
