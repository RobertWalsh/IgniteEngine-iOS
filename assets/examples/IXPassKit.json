{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "#ffffff",
            "padding.top": 70,
            "status_bar_style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control_location": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXPassKit"
                }
            },
            {
                "_id": "passKitTest",
                "_type": "PassKit",
                "actions": [
                    {
                        "_type": "Alert",
                        "attributes": {
                            "title": "Pass created."
                        },
                        "on": "success"
                    },
                    {
                        "_type": "Alert",
                        "attributes": {
                            "title": "Pass failed."
                        },
                        "on": "error"
                    }
                ],
                "attributes": {
                    "pass.location": "../data/boardingpass.pkpass"
                }
            },
            {
                "_id": "button",
                "_type": "Button",
                "actions": [
                    {
                        "_type": "Function",
                        "attributes": {
                            "_target": "passKitTest",
                            "functionName": "present"
                        },
                        "on": "touchUp"
                    }
                ],
                "attributes": {
                    "align.h": "center",
                    "align.v": "middle",
                    "bg.color": "cdcdcd",
                    "border.radius": 0,
                    "color": "6c6c6c",
                    "layoutType": "absolute",
                    "size.h": 50,
                    "size.w": 200,
                    "text": "Now boarding!",
                    "touch.bg.color": "cdcdcd",
                    "touch.color": "6c6c6c50"
                }
            }
        ],
        "datasources": []
    }
}