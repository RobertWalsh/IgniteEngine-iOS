                        "_type": "Button", 
                        "actions": [
                            {
                                "_type": "Function", 
                                "attributes": {
                                    "_target": "MultilineTextInput", 
                                    "function_name": "keyboard_hide"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "cdcdcd", 
                            "border.size": 2, 
                            "color": "6c6c6c", 
                            "size.h": 50, 
                            "size.w": 135, 
                            "touch.background.color": "cdcdcd", 
                            "touch.text.color": "6c6c6c50"
                        }
