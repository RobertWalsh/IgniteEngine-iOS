{
    "view": {
        "actions": [], 
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "TextInput", 
                "_type": "TextInput", 
                "actions": [
                    {
                        "_type": "Modify", 
                        "attributes": {
                            "_target": "session"
                        }, 
                        "on": "text_changed,got_focus", 
                        "set": {
                            "text_to_speak": "[[$self.text]]"
                        }
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "border.size": 2, 
                    "color": "#6c6c6c", 
                    "multiline.enabled": false, 
                    "placeholder.text": "Placeholder Text", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "text.align": "center"
                }
            }, 
            {
                "_id": "layoutButtons", 
                "_type": "Layout", 
                "attributes": {
                    "align.h": "center", 
                    "layoutFlow": "horizontal", 
                    "size.h": 50, 
                    "size.w": 280
                }, 
                "controls": [
                    {
                        "_id": "button", 
                        "_type": "Button", 
                        "actions": [
                            {
                                "_type": "Function", 
                                "attributes": {
                                    "_target": "TextInput", 
                                    "function_name": "keyboard_show"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "cdcdcd", 
                            "border.size": 2, 
                            "color": "6c6c6c", 
                            "size.h": 50, 
                            "size.w": 135, 
                            "touch.background.color": "cdcdcd", 
                            "touch.text.color": "6c6c6c50"
                        }
                    }, 
                    {
                        "_id": "button", 
                        "_type": "Button", 
                        "actions": [
                            {
                                "_type": "Function", 
                                "attributes": {
                                    "_target": "TextInput", 
                                    "function_name": "keyboard_hide"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "cdcdcd", 
                            "border.size": 2, 
                            "color": "6c6c6c", 
                            "size.h": 50, 
                            "size.w": 135, 
                            "touch.background.color": "cdcdcd", 
                            "touch.text.color": "6c6c6c50"
                        }
                    }
                ]
            }, 
            {
                "_id": "MultilineTextInput", 
                "_type": "TextInput", 
                "actions": [
                    {
                        "_type": "Modify", 
                        "attributes": {
