{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "#ffffff",
            "padding.top": 70,
            "status_bar_style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control_location": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXSlider"
                }
            },
            {
                "_id": "sliderTest",
                "_type": "Slider",
                "actions": [
                    {
                        "_type": "Refresh",
                        "attributes": {
                            "_target": "title"
                        },
                        "on": "valueChanged"
                    }
                ],
                "attributes": {
                    "align.h": "center",
                    "align.v": "middle",
                    "layoutType": "absolute",
                    "size.w": 280
                }
            },
            {
                "_id": "button",
                "_type": "Button",
                "actions": [
                    {
                        "_type": "Function",
                        "attributes": {
                            "_target": "sliderTest",
                            "functionName": "setValue"
                        },
                        "on": "touchUp",
                        "set": {
                            "value": 0.75
                        }
                    }
                ],
                "attributes": {
                    "align.h": "center",
                    "align.v": "bottom",
                    "bg.color": "cdcdcd",
                    "border.radius": 0,
                    "color": "6c6c6c",
                    "layoutType": "absolute",
                    "margin.bottom": 200,
                    "size.h": 50,
                    "size.w": 225,
                    "text": "Change slider to 0.75",
                    "touch.bg.color": "cdcdcd",
                    "touch.color": "6c6c6c50"
                }
            },
            {
                "_id": "title",
                "_type": "Text",
                "attributes": {
                    "align.h": "center",
                    "align.v": "bottom",
                    "color": "6c6c6c",
                    "font": "HelveticaNeue:15",
                    "layoutType": "float",
                    "margin.bottom": 15,
                    "size.h": 50,
                    "size.w": 200,
                    "text": "IXSlider value: [[sliderTest.value]]"
                }
            }
        ],
        "datasources": []
    }
}