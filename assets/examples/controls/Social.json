{
    "view": {
        "attributes": {
            "_id": "test",
            "bg.color": "#ffffff",
            "padding.top": 70,
            "status_bar_style": "dark"
        },
        "controls": [
            {
                "_id": "customHeader",
                "_type": "Custom",
                "attributes": {
                    "control_location": "../custom/header.json",
                    "layoutType": "float",
                    "margin.top": 20,
                    "size.h": 50,
                    "size.w": "100%",
                    "title": "IXSocial"
                }
            },
            {
                "_id": "socialTest",
                "_type": "Social",
                "actions": [
                    {
                        "_type": "Alert",
                        "attributes": {
                            "title": "success"
                        },
                        "on": "success"
                    }
                ],
                "attributes": {
                    "image": "http://images.sodahead.com/slideshows/000020095/1537637670_ducklips87y6-103619834647_xlarge.png",
                    "platform": "twitter",
                    "text": "I can't wait for you all to see this pic!",
                    "url": "http://duck.lips"
                }
            },
            {
                "_id": "button",
                "_type": "Button",
                "actions": [
                    {
                        "_type": "Function",
                        "attributes": {
                            "_target": "socialTest",
                            "functionName": "present"
                        },
                        "on": "touchUp"
                    }
                ],
                "attributes": {
                    "align.h": "center",
                    "align.v": "middle",
                    "bg.color": "cdcdcd",
                    "border.radius": 0,
                    "color": "6c6c6c",
                    "layoutType": "absolute",
                    "size.h": 50,
                    "size.w": 200,
                    "text": "Share on Twitter",
                    "touch.bg.color": "cdcdcd",
                    "touch.color": "6c6c6c50"
                }
            },
            {
                "_id": "title",
                "_type": "Text",
                "attributes": {
                    "align.h": "center",
                    "align.v": "bottom",
                    "color": "6c6c6c",
                    "font": "HelveticaNeue:15",
                    "layoutType": "float",
                    "margin.bottom": 15,
                    "size.h": 50,
                    "size.w": 200,
                    "text": "IXSocial: Twitter Available? \n[[socialTest.isAllowed.twitter]]",
                    "text.align": "center"
                }
            }
        ],
        "datasources": []
    }
}