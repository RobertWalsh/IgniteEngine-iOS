{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 90,
      "bg.color":"#ffffff",
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXButton"
        }
      },
      {
        "_id": "button",
        "_type": "Button",
        "actions": [
          {
            "_type": "Alert",
            "on": "touchUp",
            "attributes": {
              "title": "touchUp",
              "message": "You touched the button!"
            }
          }
        ],
        "attributes": {
          "size.w": 100,
          "size.h": 50,
          "color": "6c6c6c",
          "layoutType": "absolute",
          "bg.color": "cdcdcd",
          "touch.color": "6c6c6c50",
          "align.h": "center",
          "align.v": "middle",
          "touch.bg.color": "cdcdcd",
          "border.radius": 0,
          "text": "Hi."
        }
      }
    ],
    "datasources": []
  }
}