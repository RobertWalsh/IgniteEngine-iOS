{
    "view": {
        "actions": [
            {
                "_type": "Function", 
                "attributes": {
                    "_target": "TextInput", 
                    "function_name": "keyboard_show"
                }, 
                "on": "did_appear"
            }
        ], 
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "actionSheetTest", 
                "_type": "ActionSheet", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "cancel_pressed"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "other_pressed"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "someOther2_pressed"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "destructiveButtonTitle_pressed"
                    }
                ], 
                "attributes": {
                    "buttons.cancel": "cancelButtonTitle", 
                    "buttons.destructive": "destructiveButtonTitle", 
                    "buttons.others": "other,someOther2", 
                    "sheet.style": "black.opaque", 
                    "title": "sheetTitle"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "actionSheetTest", 
                            "function_name": "show_sheet"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "bg.color": "cdcdcd", 
                    "border.size": 2, 
                    "color": "6c6c6c", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }
        ], 
        "data_providers": []
    }
}