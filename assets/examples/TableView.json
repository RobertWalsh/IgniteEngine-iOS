{
    "view": {
        "actions": [], 
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "tableSettings", 
                "_type": "TableView", 
                "attributes": {
                    "autofill.h": true, 
                    "autofill.w": true, 
                    "bg.color": "[[app.color]]", 
                    "cell.size.h": 50, 
                    "cell.size.w": "100%", 
                    "color.background": "#5A5A5A", 
                    "datasource.id": "test_data"
                }, 
                "controls": [
                    {
                        "_type": "Layout", 
                        "actions": [
                            {
                                "_type": "Alert", 
                                "attributes": {}, 
                                "on": "touch_up"
                            }, 
                            {
                                "_type": "Navigate", 
                                "attributes": {
                                    "nav_animation_duration": 0, 
                                    "nav_animation_type": "cross_dissolve", 
                                    "nav_stack_type": "push", 
                                    "to": "Social.json"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "align.v": "middle", 
                            "layoutFlow": "horizontal", 
                            "layoutType": "absolute"
                        }, 
                        "controls": [
                            {
                                "_type": "Text", 
                                "attributes": {
                                    "color": "#6c6c6c"
                                }
                            }, 
                            {
                                "_type": "Text", 
                                "attributes": {
                                    "color": "#6c6c6c"
                                }
                            }
                        ]
                    }, 
                    {
                        "_type": "Image", 
                        "attributes": {
                            "align.h": "right", 
                            "image": "../images/btn_chevron_50x50.png", 
                            "layoutType": "absolute", 
                            "size.h": 50, 
                            "size.w": 50
                        }
                    }, 
                    {
                        "_type": "Layout", 
                        "attributes": {
                            "align.v": "bottom", 
                            "bg.color": "#cdcdcd", 
                            "layoutType": "float", 
                            "size.h": 1, 
                            "size.w": "100%"
                        }
                    }
                ]
            }
        ], 
        "data_providers": [
            {
                "_id": "test_data", 
                "_type": "JSON", 
                "actions": [
                    {
                        "_type": "Refresh", 
                        "attributes": {
                            "_target": "tableSettings"
                        }, 
                        "on": [
                            "success", 
                            "fail"
                        ]
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "fail,success"
                    }
                ], 
                "attributes": {
                    "auto_load": true, 
                    "data.basepath": "", 
                    "data.baseurl": "../data/states.json", 
                    "http_method": "GET", 
                    "objects_path": ""
                }
            }
        ]
    }
}