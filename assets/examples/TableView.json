{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 70,
      "bg.color": "#f8f8f8",
      "status_bar_style": "dark"
    },
    "actions": [
      
    ],
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "../custom/header.json",
          "layoutType": "float",
          "size.h": 50,
          "size.w": "100%",
          "margin.top": 20,
          "title": "IXTableView"
        }
      },
      {
        "_id": "tableDemo",
        "_type": "TableView",
        "attributes": {
          "size.h": "100%",
          "size.w": 320,
          "data.basepath": "entities",
          "separator.style": "none",
          "bg.color": "#ff00ff00",
          "datasource.id": "dataMenu",
          "padding.bottom": 0,
          "enabled": true,
          "cell.size.h": 42,
          "margin.top": 0,
          "animate_reload": false
        },
        "controls": [
          {
            "_id": "buttonEmployeeList",
            "_type": "Layout",
            "actions": [
              {
                "_type": "Navigate",
                "attributes": {
                  "to": "[[dataRow.name]].json",
                  "animation.type": "crossDissolve",
                  "animation.duration": 0
                },
                "on": "touchUp"
              },
              {
                "_type": "Event",
                "attributes": {
                  "delay": 0.5,
                  "_target": "imageMenu",
                  "event_name": "close"
                },
                "on": "touchUp"
              }
            ],
            "attributes": {
              "alpha": 1,
              "font": "[[app.font]]:20",
              "padding.left": 0,
              "size.w": 320,
              "size.h": 42
            },
            "controls": [
              {
                "_id": "textCategory",
                "_type": "Text",
                "actions": [],
                "attributes": {
                  "alpha": 1,
                  "font": "FontAwesome:16",
                  "text": "[[dataRow.icon]]",
                  "align.v": "middle",
                  "layoutType": "absolute",
                  "color": "#8f8f8f",
                  "padding.left": 20,
                  "touch.color": "[[app.color.touch]]",
                  "size.w": 60
                }
              },
              {
                "_id": "textCategory",
                "_type": "Text",
                "actions": [],
                "attributes": {
                  "alpha": 1,
                  "font": "HelveticaNeue-Light:18",
                  "text": "[[dataRow.name]]",
                  "align.v": "middle",
                  "layoutType": "absolute",
                  "padding.left": 60,
                  "color": "#8f8f8f",
                  "touch.color": "[[app.color.touch]]",
                  "size.w": "100%"
                }
              }
            ]
          }
        ]
      }
    ],
    "datasources": [
      {
        "_id": "dataMenu",
        "_type": "JSON",
        "actions": [
          {
            "_type": "Alert",
            "attributes": {
              "message": "Category: [[session.category_id]]\n\nRow Count: [[$self.count]]",
              "title": "admin_orders.json success",
              "enabled": false
            },
            "on": "success"
          },
          {
            "_type": "Refresh",
            "attributes": {
              "_target": "tableDemo"
            },
            "on": "success"
          }
        ],
        "attributes": {
          "autoLoad.enabled": true,
          "baseUrl": "../data",
          "pathSuffix": "menu.json",
          "sort.order": "ascending",
          "sort.key": "name",
          "data.basepath": "entities",
          "cache.id": "menu",
          "http.method": "GET"
        },
        "parameters": {}
      }
    ]
  }
}