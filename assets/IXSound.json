{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "soundTest", 
                "_type": "Sound", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {
                            "message": "Played sound: /resources/sounds/powerup.caf"
                        }, 
                        "on": "finished"
                    }
                ], 
                "attributes": {
                    "audioUrl": "/resources/sounds/powerup.caf", 
                    "autoPlay.enabled": false, 
                    "repeatCount": 0
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "soundTest", 
                            "function_name": "play"
                        }, 
                        "on": "touch_up"
                    }, 
                    {
                        "_type": "Refresh", 
                        "attributes": {
                            "_target": "title", 
                            "delay": 0.25
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "bg.color": "cdcdcd", 
                    "color": "6c6c6c", 
                    "layoutType": "absolute", 
                    "size.h": 50, 
                    "size.w": 200, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }, 
            {
                "_id": "title", 
                "_type": "Text", 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "bottom", 
                    "color": "6c6c6c", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": 280, 
                    "text.align": "center"
                }
            }
        ], 
        "data_providers": []
    }
}