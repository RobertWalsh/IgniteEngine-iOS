{
  "$app": {
    "actions": [],
    "attributes": {
      "_OPTIONAL:drawer.toggle.velocity": 1000,
      "_OPTIONAL:drawer.view.right": "examples/TableView.json",
      "_OPTIONAL:preload_images": [
        "assets/images/pin_32x30.png",
        "assets/images/pin_32x30@2x.png"
      ],
      "apigee.app": "***APIGEE_APP_ID***",
      "apigee.org": "***APIGEE_ORG_ID***",
      "apigee.notifier": "***APIGEE_PUSH_NOTIFIER***",
      "zview.index": "examples/datasources/OAuth2.json",      
      "view.index": "watch.json",
      "1drawerController.l.url": "menu.json",
      "1drawerController.l.max.w": 275,      
      "debug.layout.enabled": false,
      "logging.datasource.enabled": true,
      "logging.level": "verbose",
      "mode": "verbose",
      "search_query": "Santa Barbara, CA",
      "navigationBar.enabled": false
    },
    "datasources": [
      
      {
        "_id": "dataMenu",
        "_type": "JSON",
        "actions": [
          {
            "_type": "Alert",
            "attributes": {
              "message": "Category: [[session.category_id]]\n\nRow Count: [[$self.count]]",
              "title": "admin_orders.json success",
              "enabled": false
            },
            "on": "success"
          },
          {
            "_type": "Refresh",
            "attributes": {
              "_target": "tableDemo"
            },
            "on": "success"
          }
        ],
        "attributes": {
          "autoLoad.enabled": true,
          "baseUrl": "/data",
          "pathSuffix": "menu.json",
          "sort.order": "ascending",
          "sort.key": "name",
          "data.basepath": "entities",
          "cache.id": "menu",
          "http.method": "GET"
        },
        "parameters": {}
      }
    ]
  }
}