{
  "view": {
    "attributes": {
      "_id": "test",
      "padding.top": 90,
      "background": {
        "color": "#f8f8f8"
      },
      "status_bar_style": "dark"
    },
    "controls": [
      {
        "_id": "customHeader",
        "_type": "Custom",
        "attributes": {
          "control_location": "custom/header.json",
          "layout_type": "float",
          "height": 50,
          "width": "100%",
          "margin.top": 20,
          "title": "IXFunctionAction"
        }
      },
      {
        "_id": "layoutButtons",
        "_type": "Layout",
        "attributes": {
          "layout_type": "absolute",
          "horizontal_alignment": "center",
          "vertical_alignment": "middle"
        },
        "controls": [
          {
            "_id": "button0",
            "_type": "Button",
            "actions": [
              {
                "_type": "Function",
                "on": "touch_up",
                "attributes": {
                  "_target": "actionSheetTest",
                  "function_name": "show_sheet"
                }
              }
            ],
            "attributes": {
              "width": 280,
              "height": 50,
              "text.color": "6c6c6c",
              "background.color": "cdcdcd",
              "touch.text.color": "6c6c6c50",
              "touch.background.color": "cdcdcd",
              "margin.top": 15,
              "text": "function: show_sheet"
            }
          }
        ]
      },
      {
        "_type": "ActionSheet",
        "_id": "actionSheetTest",
        "attributes": {
          "sheet.style": "black.opaque",
          "sheet.title": "sheetTitle",
          "sheet.button.title.cancel": "cancelButtonTitle",
          "sheet.button.title.destructive": "destructiveButtonTitle",
          "sheet.button.title.others": "other,someOther2"
        },
        "actions": [
          {
            "on": "cancel_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "Cancel Pressed"
            }
          },
          {
            "on": "other_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "other pressed [[app.bundle.version]]"
            }
          },
          {
            "on": "someOther2_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "someOther2 pressed"
            }
          },
          {
            "on": "[destructiveButtonTitle](#//api/name/destructiveButtonTitle)_pressed",
            "_type": "Alert",
            "attributes": {
              "title": "[destructiveButtonTitle](#//api/name/destructiveButtonTitle) pressed"
            }
          }
        ]
      }
    ],
    "data_providers": []
  }
}