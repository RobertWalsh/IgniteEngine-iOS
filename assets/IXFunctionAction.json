{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "layoutButtons", 
                "_type": "Layout", 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "layoutType": "absolute"
                }, 
                "controls": [
                    {
                        "_id": "button0", 
                        "_type": "Button", 
                        "actions": [
                            {
                                "_type": "Function", 
                                "attributes": {
                                    "_target": "actionSheetTest", 
                                    "function_name": "show_sheet"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "cdcdcd", 
                            "color": "6c6c6c", 
                            "size.h": 50, 
                            "size.w": 280, 
                            "touch.background.color": "cdcdcd", 
                            "touch.text.color": "6c6c6c50"
                        }
                    }
                ]
            }, 
            {
                "_id": "actionSheetTest", 
                "_type": "ActionSheet", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "cancel_pressed"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "other_pressed"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "someOther2_pressed"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "[destructiveButtonTitle](#//api/name/destructiveButtonTitle)_pressed"
                    }
                ], 
                "attributes": {
                    "buttons.cancel": "cancelButtonTitle", 
                    "buttons.destructive": "destructiveButtonTitle", 
                    "buttons.others": "other,someOther2", 
                    "sheet.style": "black.opaque", 
                    "title": "sheetTitle"
                }
            }
        ], 
        "data_providers": []
    }
}