{
    "view": {
        "actions": [
            {
                "_type": "Function", 
                "attributes": {
                    "_target": "cameraTest", 
                    "function_name": "start"
                }, 
                "on": "did_appear"
            }
        ], 
        "attributes": {
            "_id": "IXCamera", 
            "status_bar_style": "light"
        }, 
        "controls": [
            {
                "_id": "cameraTest", 
                "_type": "Camera", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "did_capture_image"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "did_finish_saving_capture"
                    }
                ], 
                "attributes": {
                    "cameraSource": "rear", 
                    "size.h": "100%", 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "cameraTest", 
                            "function_name": "capture_image"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "bg.color": "cdcdcd", 
                    "color": "6c6c6c", 
                    "layoutType": "absolute", 
                    "size.h": 50, 
                    "size.w": 150, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }
        ]
    }
}