{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "calendarTest", 
                "_type": "Calendar", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "add_event_success"
                    }, 
                    {
                        "_type": "Alert", 
                        "attributes": {}, 
                        "on": "add_event_failed"
                    }
                ], 
                "attributes": {
                    "alarmOffset": -1800, 
                    "allDay.enabled": false, 
                    "date.end": "2016-01-01 00:00:00", 
                    "date.format": "yyyy-MM-dd HH:mm:ss", 
                    "date.start": "2015-12-31 23:00:00", 
                    "location": "1234 Some Street, City, State Zip", 
                    "notes": "Event Notes", 
                    "repeatFrequency": "none", 
                    "title": "Event Title", 
                    "url": "http://meetings-are-fun.com"
                }
            }, 
            {
                "_id": "button", 
                "_type": "Button", 
                "actions": [
                    {
                        "_type": "Function", 
                        "attributes": {
                            "_target": "calendarTest", 
                            "function_name": "add_event"
                        }, 
                        "on": "touch_up"
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "middle", 
                    "bg.color": "cdcdcd", 
                    "color": "6c6c6c", 
                    "layoutType": "absolute", 
                    "size.h": 80, 
                    "size.w": 280, 
                    "touch.background.color": "cdcdcd", 
                    "touch.text.color": "6c6c6c50"
                }
            }
        ], 
        "data_providers": []
    }
}