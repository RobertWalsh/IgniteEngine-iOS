{
    "view": {
        "attributes": {
            "_id": "test", 
            "background": {}, 
            "status_bar_style": "dark"
        }, 
        "controls": [
            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "tableSettings", 
                "_type": "TableView", 
                "attributes": {
                    "bg.color": "#ff00ff00", 
                    "cell.size.h": 42, 
                    "datasource.id": "dataMenu", 
                    "reloadAnimation.enabled": false, 
                    "size.h": "100%", 
                    "size.w": "100%"
                }, 
                "controls": [
                    {
                        "_id": "buttonEmployeeList", 
                        "_type": "Layout", 
                        "actions": [
                            {
                                "_type": "Navigate", 
                                "attributes": {
                                    "to": "[[dataRow.name]].json"
                                }, 
                                "on": "touch_up"
                            }, 
                            {
                                "_type": "Event", 
                                "attributes": {
                                    "_target": "imageMenu", 
                                    "delay": 0.5, 
                                    "event_name": "close"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "size.h": 42
                        }, 
                        "controls": [
                            {
                                "_id": "textCategory", 
                                "_type": "Text", 
                                "actions": [], 
                                "attributes": {
                                    "align.v": "middle", 
                                    "color": "#8f8f8f", 
                                    "layoutType": "absolute", 
                                    "size.w": 60, 
                                    "touch.text.color": "[[app.color.touch]]"
                                }
                            }, 
                            {
                                "_id": "textCategory", 
                                "_type": "Text", 
                                "actions": [], 
                                "attributes": {
                                    "align.v": "middle", 
                                    "color": "#8f8f8f", 
                                    "layoutType": "absolute", 
                                    "size.w": "100%", 
                                    "touch.text.color": "[[app.color.touch]]"
                                }
                            }
                        ]
                    }
                ]
            }, 
            {
                "_id": "layoutButtons", 
                "_type": "Layout", 
                "attributes": {
                    "align.h": "center", 
                    "align.v": "bottom", 
                    "layoutType": "absolute"
                }, 
                "controls": [
                    {
                        "_id": "button0", 
                        "_type": "Button", 
                        "actions": [
                            {
                                "_type": "Load", 
                                "attributes": {
                                    "_target": "dataMenu"
                                }, 
                                "on": "touch_up"
                            }
                        ], 
                        "attributes": {
                            "bg.color": "cdcdcd", 
                            "color": "6c6c6c", 
                            "size.h": 50, 
                            "size.w": 280, 
                            "touch.background.color": "cdcdcd", 
                            "touch.text.color": "6c6c6c50"
                        }
                    }
                ]
            }
        ], 
        "data_providers": [
            {
                "_id": "dataMenu", 
                "_type": "JSON", 
                "actions": [
                    {
                        "_type": "Alert", 
                        "attributes": {
                            "messade": "Category: [[session.category_id]]\n\nRow Count: [[$self.count]]"
                        }, 
                        "on": "success"
                    }
                ], 
                "attributes": {
                    "auto_load": false, 
                    "cache_id": "menu", 
                    "data.basepath": "entities", 
                    "data.baseurl": "/data", 
                    "data.path": "menu.json", 
                    "http_method": "GET", 
                    "sort.key": "name", 
                    "sort.order": "ascending"
                }, 
                "parameters": {}
            }
        ]
    }
}