            {
                "_id": "customHeader", 
                "_type": "Custom", 
                "attributes": {
                    "control_location": "custom/header.json", 
                    "layoutType": "float", 
                    "size.h": 50, 
                    "size.w": "100%"
                }
            }, 
            {
                "_id": "textInputTest", 
                "_type": "TextInput", 
                "actions": [
                    {
                        "_type": "Modify", 
                        "attributes": {
                            "_target": "session"
                        }, 
                        "on": "text_changed,got_focus", 
                        "set": {
                            "text_to_speak": "[[$self.text]]"
                        }
                    }
                ], 
                "attributes": {
                    "align.h": "center", 
                    "border.size": 1, 
                    "color": "#6c6c6c", 
                    "multiline.enabled": false, 
